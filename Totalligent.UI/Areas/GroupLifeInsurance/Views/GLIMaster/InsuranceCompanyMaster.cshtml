    function DeActiveICMaster(obj, RID) {

        var RIDs = '' + RID;
        var param = { RiMasterID: RIDs };
        $.ajax({
            type: "POST",
            url: "/GroupLifeInsurance/InsuranceCompanyMaster/DeactivateICMaster",
            dataType: "json",
            contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
            data: param,
            success: function (Result) {
                if (Result == "DeActivated") {
                    $(obj).closest('tr').remove();
                    alert("DeActivated successfully");
                    return false;
                }
                else { alert(Result); }
            },
            error: function () {
                console.log('Error while deactivating!.');
            }
        })

    }
