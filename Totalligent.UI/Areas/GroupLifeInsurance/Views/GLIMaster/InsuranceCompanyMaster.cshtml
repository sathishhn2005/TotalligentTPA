    function csvFileUpload() {

        var rowCount = 0;
        var regex = /^([a-zA-Z0-9\s_\\.\-:])+(.csv|.txt)$/;
        if (regex.test($("#fileUploadICM").val().toLowerCase())) {

            if (typeof (FileReader) != "undefined") {
                var reader = new FileReader();
                reader.onload = function (e) {

                    var rows = e.target.result.split('\r\n');

                    for (var i = 0; i < rows.length; i++) {

                        var cells = rows[i].split(",");
                        var RRC = rowCount + 1;
                        if (i == 0) {


                            // Heater validation
                            if (cells.length == 11) {

                                if (cells[0] != "InsuranceCompanyName") {
                                    $('#fileUploadICM').val('');
                                    alert("Please check column name mismatch (InsuranceCompanyName).");
                                    return false;
                                }
                                if (cells[1] != "ContactPerson") {
                                    $('#fileUploadICM').val('');
                                    alert("Please check column name mismatch (ContactPerson).");
                                    return false;
                                }
                                if (cells[2] != "MobileNumber") {
                                    $('#fileUploadICM').val('');
                                    alert("Please check column name mismatch (MobileNumber).");
                                    return false;
                                }
                                if (cells[3] != "EmailId") {
                                    $('#fileUploadICM').val('');
                                    alert("Please check column name mismatch (EmailID).");
                                    return false;
                                }
                                if (cells[4] != "Address") {
                                    $('#fileUploadICM').val('');
                                    alert("Please check column name mismatch (Address).");
                                    return false;
                                }
                                if (cells[5] != "City") {
                                    $('#fileUploadICM').val('');
                                    alert("Please check column name mismatch (City).");
                                    return false;
                                }
                                if (cells[6] != "State") {
                                    $('#fileUploadICM').val('');
                                    alert("Please check column name mismatch (State).");
                                    return false;
                                }
                                if (cells[7] != "Zipcode") {
                                    $('#fileUploadICM').val('');
                                    alert("Please check column name mismatch (Zipcode).");
                                    return false;
                                }
                                if (cells[8] != "BankName") {
                                    $('#fileUploadICM').val('');
                                    alert("Please check column name mismatch (BankName).");
                                    return false;
                                }
                                if (cells[9] != "AccountNumber") {
                                    $('#fileUploadICM').val('');
                                    alert("Please check column name mismatch (AccountNumber).");
                                    return false;
                                }
                                if (cells[10] != "IFSCCode") {
                                    $('#fileUploadICM').val('');
                                    alert("Please check column name mismatch (IFSCCode).");
                                    return false;
                                }


                            }
                            else {
                                $('#fileUploadICM').val('');
                                alert("Upload Csv file invalid template , Please check .");
                                return false;
                            }

                        } else {
                            // Row validation
                            if (cells.length == 11) {


                                if (cells[0] == "") {
                                    $('#fileUploadICM').val('');
                                    alert("Please check Row NO : " + RRC + " InsuranceCompanyName  is empty");
                                    return false;
                                }

                                if (cells[1] == "") {
                                    $('#fileUploadICM').val('');
                                    alert("Please check Row NO : " + RRC + " ContactPerson is empty");
                                    return false;
                                }

                                if (cells[2] == "") {
                                    $('#fileUploadICM').val('');
                                    alert("Please check Row NO : " + RRC + " MobileNumber  is empty");
                                    return false;
                                }

                                if (cells[3] == "") {
                                    $('#fileUploadICM').val('');
                                    alert("Please check Row NO : " + RRC + " EmailID is empty");
                                    return false;
                                }

                                if (cells[4] == "") {
                                    $('#fileUploadICM').val('');
                                    alert("Please check Row NO : " + RRC + " Address is empty");
                                    return false;
                                }
                                if (cells[5] == "") {
                                    $('#fileUploadICM').val('');
                                    alert("Please check Row NO : " + RRC + " City is empty");
                                    return false;
                                }

                                if (cells[6] == "") {
                                    $('#fileUploadICM').val('');
                                    alert("Please check Row NO : " + RRC + " State is empty");
                                    return false;
                                }
                                if (cells[7] == "") {
                                    $('#fileUploadICM').val('');
                                    alert("Please check Row NO : " + RRC + " Zipcode is empty");
                                    return false;
                                }
                                if (cells[8] == "") {
                                    $('#fileUploadICM').val('');
                                    alert("Please check Row NO : " + RRC + " BankName is empty");
                                    return false;
                                }

                                if (cells[9] == "") {
                                    $('#fileUploadICM').val('');
                                    alert("Please check Row NO : " + RRC + " AccountNumber is empty");
                                    return false;
                                }
                                if (cells[10] == "") {
                                    $('#fileUploadICM').val('');
                                    alert("Please check Row NO : " + RRC + " IFSCCode is empty");
                                    return false;
                                }



                            }
                            else {

                                if (cells.length > 1) {
                                    $('#fileUploadICM').val('');
                                    alert("Please check Row NO : " + RRC + " This row have comma simple or invalied Column length");
                                    return false;
                                }
                            }

                        }

                        rowCount += 1;

                    }

                    if (rowCount == rows.length) {
                        $("#hdnMsgStatus").val('Y');
                        $("#uploadFiles").click();

                    }

                }
                reader.readAsText($("#fileUploadICM")[0].files[0]);
            } else {
                $('#fileUploadICM').val('');
                alert("This browser does not support HTML5.");
                return false;
            }
        } else {
            $('#fileUploadICM').val('');
            alert("Please upload a valid CSV file.");
            return false;
        }
    }
