@model Totalligent.UI.Models.ICMasterModel
@using Totalligent.BusinessEntities;
@{
    ViewBag.Title = "InsuranceCompanyMaster";
    Layout = "~/Areas/GroupLifeInsurance/Views/Shared/_Layout.cshtml";
}

@Html.AntiForgeryToken()
<div class="p-t-30">
    <!-- Step wizard -->
    <div class="box box-default">
        <div class="box-header with-border">
            <ul class="nav nav-pills justify-content-center mb-0">
                <li class=" nav-item flex-fill text-center"> <a href="#navpills2-1" class="nav-link active" data-toggle="tab" aria-expanded="false"><i class="fa fa-building-o mr-3"></i>Insurance Company Master</a> </li>
                <li class="nav-item flex-fill text-center"> <a href="#navpills2-2" class="nav-link" data-toggle="tab" aria-expanded="false"><i class="fa fa-search mr-3"></i>Search Insurance Company Master</a> </li>
            </ul>
        </div>
        <!-- /.box-header -->
        <div class="box-body">
            <div class="tab-content">
                <div id="navpills2-1" class="tab-pane active">
                    <form id="formICMaster">
                       <h4 class="text-center text-success">@TempData["Alertmsg"]</h4> 
                        @{ TempData.Keep("ICFolderName");}
                        <div class="row">
                            <div class="col-12 col-md-6">
                                <div class="form-group m-b-15">
                                    <div class="row">
                                        <div class="col-12 col-md-10">
                                            <h4 class="col-sm-12 font-weight-bold text-center">Insurance Company Basic Details</h4>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-horizontal">
                                    <div class="row m-b-15">

                                        <label for="InsurancecompanyName" class="col-sm-4 col-form-label text-md-right w-full">Insurance Company Name</label>
                                        <div class="col-md-6">
                                            @Html.TextBox("InsurancecompanyName", "", new { @required = "required", @class = "form-control", placeholder = "Enter an Insurance Company Name" })

                                        </div>
                                    </div>
                                    <div class="row m-b-15">
                                        @*@Html.LabelFor(model => model.ContactPerson, htmlAttributes: new { @class = "col-sm-4 col-form-label text-md-right w-full" })*@
                                        <label for="ContactPerson" class="col-sm-4 col-form-label text-md-right w-full">ContactPerson</label>
                                        <div class="col-md-6">
                                            @Html.TextBox("ContactPerson", "", new { @required = "required", @class = "form-control", placeholder = "Enter a Contact Person Name" })
                                        </div>
                                    </div>
                                    <div class="row m-b-15">
                                        <label for="MobileNumber" class="col-sm-4 col-form-label text-md-right w-full">MobileNumber</label>
                                        <div class="col-md-6">
                                            @Html.TextBox("MobileNumber", "", new { @required = "required", @class = "form-control", placeholder = "Enter Mobile Number" })
                                        </div>
                                    </div>
                                    <div class="row m-b-15">
                                        <label for="EmailId" class="col-sm-4 col-form-label text-md-right w-full">EmailId</label>
                                        <div class="col-md-6">
                                            @Html.TextBox("EmailId", "", new { @required = "required", @class = "form-control", placeholder = "Enter Email Id", type = "email" })
                                        </div>
                                    </div>
                                    <div class="row m-b-15">
                                        <label for="Address" class="col-sm-4 col-form-label text-md-right w-full">Address</label>
                                        <div class="col-md-6">
                                            @Html.TextBox("Address", "", new { @required = "required", @class = "form-control", placeholder = "Enter an Address Name" })
                                        </div>
                                    </div>
                                    <div class="row m-b-15">
                                        <label for="City" class="col-sm-4 col-form-label text-md-right w-full">City</label>
                                        <div class="col-md-6">
                                            @Html.TextBox("City", "", new { @required = "required", @class = "form-control", placeholder = "Enter City Name" })
                                        </div>
                                    </div>
                                    <div class="row m-b-15">
                                        <label for="State" class="col-sm-4 col-form-label text-md-right w-full">State</label>
                                        <div class="col-md-6">
                                            @Html.TextBox("State", "", new { @required = "required", @class = "form-control", placeholder = "Enter State Name" })
                                        </div>
                                    </div>
                                    <div class="row m-b-15">
                                        <label for="Zipcode" class="col-sm-4 col-form-label text-md-right w-full">Zipcode</label>
                                        <div class="col-md-6">
                                            @Html.TextBox("Zipcode", "", new { @required = "required", @class = "form-control", placeholder = "Enter Zipcode" })
                                        </div>
                                    </div>

                                    <div class="row d-none">
                                        <div class="col-12">
                                            @using (Html.BeginForm("BulkUpload", "Agent", FormMethod.Post, new { enctype = "multipart/form-data" }))
                                            {
                                                <input type="file" name="postedFile" accept=".xls,.xlsx" />
                                                <input type="submit" value="Import" />
                                                <a href="~/Masters/InsuranceCompanyMaster.csv" download>Download Sample format</a>}
                                        </div>
                                    </div>

                                </div>
                            </div>
                            <div class="col-12 col-md-6">
                                <div class="form-group m-b-15">
                                    <div class="row">
                                        <div class="col-12 col-md-10">
                                            <h4 class="col-sm-12 font-weight-bold text-center">Client's Account Details</h4>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group m-b-15">
                                    <div class="row">
                                        <div class="col-md-5">
                                            <label for="BankName" class="col-form-label text-md-right w-full">Bank Name</label>
                                        </div>
                                        <div class="col-md-6">
                                            @Html.DropDownList("BankName", Model.lstBankMaster, "-- Select Bank --", new { @class = "form-control" })
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group m-b-15">
                                    <div class="row">
                                        <div class="col-md-5">
                                            <label class="col-form-label text-md-right w-full">Account Number</label>
                                        </div>
                                        <div class="col-md-6">
                                            @Html.TextBox("AccountNumber", "", new { @class = "form-control w-full", @placeholder = "Enter Account Number", @maxlength = "20" })
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group m-b-15">
                                    <div class="row">
                                        <div class="col-md-5">
                                            <label class="col-form-label text-md-right w-full">Repeat Account Number</label>
                                        </div>
                                        <div class="col-md-6">
                                            @Html.Password("RepeatAccountNumber", "", new { @class = "form-control w-full", @placeholder = "Enter Account Number", @maxlength = "20" })
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group m-b-15">
                                    <div class="row">
                                        <div class="col-md-5">
                                            <label class="col-form-label text-md-right w-full">IFSC Code</label>
                                        </div>
                                        <div class="col-md-6">
                                            @Html.TextBox("IFSCCode", "", new { @class = "form-control w-full", @placeholder = "Enter IFSC Code", @maxlength = "15" })
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group m-b-15">
                                    <div class="row">
                                        <div class="col-md-5">
                                            <label class="col-form-label text-md-right w-full">Upload KYC </label>
                                        </div>
                                        <div class="col-md-6">
                                            <div id="Divdropzone" class=" dropzone" action="~/GroupLifeInsurance/InsuranceCompanyMaster/SaveUploadedFile" method="post" enctype="multipart/form-data">
                                                <div id="tabs-3">
                                                    <div class="row field ">
                                                        <div id="dropzoneWrapper" class="dz-default dz-message col-12">
                                                            <i class="mdi mdi-cloud-upload font-size-30 text-muted"></i>
                                                            <h5>Drop files here or click to Upload</h5>
                                                            <div id="dropZoneContainer">

                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div id="divKYCDownload" class="hide">
                                                <a href="/KYC_Files/ICMasterKYCFiles/20041.zip" class="badge badge-pill badge-warning" download> Download KYC</a>
                                            </div>

                                        </div>
                                    </div>

                                </div>
                            </div>
                            <div class="col-12">
                                <div class="form-group m-b-15">
                                    <div class="row">
                                        <div class="col-md-12 text-center">
                                            <input type="submit" id="btnSaveInsComp" value="Create" class="btn btn-primary" />
                                            <button class="btn btn-primary-login m-20" type="button" data-toggle="modal" data-target="#uploadBulkInsurance">
                                                <i class="mdi mdi-cloud-upload"></i> Bulk Upload Policy
                                            </button>
                                            <button class="d-none btn btn-primary-login m-20" type="button" data-toggle="modal" data-target="#addNewField">
                                                <i class="mdi mdi-cloud-upload"></i> Add New Field
                                            </button>
                                        </div>
                                    </div>
                                </div>
                                <input type="hidden" id="hdnICMasterID" value="0">
                            </div>
                        </div>
                    </form>
                </div>
                <div id="navpills2-2" class="tab-pane">
                    <div class="card no-shadow">
                        <div class="card-body form-horizontal">
                            <div class="row ">
                                <div class="col-md-3">
                                    @Html.TextBox("txtCompanyName", "", new { @required = "required", @class = "form-control", placeholder = "Enter an Insurance Company Name" })
                                </div>
                                <div class="col-md-3">
                                    @Html.TextBox("txtCompanyCode", "", new { @required = "required", @class = "form-control", placeholder = "Enter an Insurance Company Code" })
                                </div>
                                <div class="col-md-3">
                                    @Html.TextBox("txtCity", "", new { @required = "required", @class = "form-control", placeholder = "Enter an Insurance Company City" })
                                </div>
                                <div class="col-md-3">
                                    <input type="hidden" id="ICFolderName" value='@TempData["ICFolderName"]' />
                                    <button type="button" id="btnSearch" class="btn btn-block btn-primary"><i class="fa fa-search mr-3"></i>Search</button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <h5>Search Results:</h5>
                    <br />
                    <div class="table-responsive">
                        <table id="tblICMaster" class="table table-hover">
                            <tr>
                                <th>ICName</th>
                                <th>ICCode</th>
                                <th>ContactPerson</th>
                                <th>MobileNumber</th>
                                <th>EmailId</th>
                                <th>Address</th>
                                
                                <th>KYCUploadPath</th>
                                <th class="text-center">Actions</th>
                            </tr>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@using (Html.BeginForm("BulkUpdate", "InsuranceCompanyMaster", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div id="uploadBulkInsurance" class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-md">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title mt-0" id="uploadBulkLabel">Upload Bulk Insurance Company</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-10 mx-auto">
                            <div class="text-right">

                                <a href="~/Masters/InsuranceCompanyMaster.csv" class="btn btn-dark btn-sm" download>
                                    <i class="mdi mdi-download"></i> Download Sample format
                                </a>
                            </div>
                            <div class="fileuploader fileuploader-theme-dragdrop">
                                <input type="file" name="ICMCSVFile" id="fileUploadICM" accept=".csv" />
                                <div class="mt-2">
                                    <i class="fa fa-upload fa-sm btn btn-success btn-block" onclick="csvFileUpload();">
                                        <span>Upload</span>
                                    </i>
                                    <button id="uploadFiles" class="btn btn-primary btn-sm mx-1 hide">Upload File</button>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
            <!-- /.modal-content -->
        </div>
        <!-- /.modal-dialog -->
    </div>
}
<div id="addNewField" class="modal fade bs-example-modal-md" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title mt-0" id="uploadBulkLabel">Add New Fields</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="form-horizontal">
                    <div class="row m-b-15">
                        <label class="col-sm-6 col-form-label text-center font-weight-bold w-full">Field Name</label>
                        <label class="col-sm-6 col-form-label text-center font-weight-bold w-full">Field Type</label>
                    </div>
                    <div class="row m-b-15">
                        <div class="col-md-6">
                            @Html.TextBox("ContactPersonNew", new { @class = "form-control", placeholder = "Enter a Contact Field Name" })
                        </div>
                        <div class="col-md-6">
                            @Html.DropDownList("BankName", Model.lstBankMaster, "-- Select Bank --", new { @class = "form-control" })
                        </div>
                    </div>
                    <div class="row m-b-15">
                        <div class="col-12 text-right">
                            <button type="button" class="btn btn-primary "><i class="mdi mdi-plus"></i> Add</button>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer text-center">
                <button id="btnClose" type="button" class="btn btn-secondary " data-dismiss="modal">Close</button>
                <button id="btnSubmitBulkUpload" type="button" class="btn btn-primary ">Submit</button>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>
<script type="text/javascript">


    $(document).ready(function () {

        $("#txtCompanyName").autocomplete({
            minLength: 2,
            autoFocus: true,
            open: function () { $($('.ui-autocomplete')).css('width', '300px'); },
            source: function (request, response) {
                var param = { prefixText: $('#txtCompanyName').val(), Action: "ICName" };

                $.ajax({
                    url: "/GroupLifeInsurance/InsuranceCompanyMaster/GetAutocompleteICNameICCodeCity",
                    data: JSON.stringify(param),
                    dataType: "json",
                    type: "POST",
                    contentType: "application/json; charset=utf-8",

                    success: function (data) {
                        response($.map(data, function (item) {
                            return {
                                label: item.InsurancecompanyName,
                                value: item.InsurancecompanyName
                            }
                        }))
                    },
                    error: function (XMLHttpRequest, textStatus, errorThrown) {
                        var err = eval("(" + XMLHttpRequest.responseText + ")");
                        //alert(err.Message)
                        console.log("Ajax Error!");
                    }
                });
            },
            //select: function (e, i) {
            //    $("#txtSeaRIMaster").val(i.item.label);
            //    return false;
            //},
        });

        $("#txtCompanyCode").autocomplete({
            minLength: 2,
            autoFocus: true,
            open: function () { $($('.ui-autocomplete')).css('width', '300px'); },
            source: function (request, response) {
                var param = { prefixText: $('#txtCompanyCode').val(), Action: "ICCode" };

                $.ajax({
                    url: "/GroupLifeInsurance/InsuranceCompanyMaster/GetAutocompleteICNameICCodeCity",
                    data: JSON.stringify(param),
                    dataType: "json",
                    type: "POST",
                    contentType: "application/json; charset=utf-8",

                    success: function (data) {
                        response($.map(data, function (item) {
                            return {
                                label: item.InsCompCode,
                                value: item.InsCompCode
                            }
                        }))
                    },
                    error: function (XMLHttpRequest, textStatus, errorThrown) {
                        var err = eval("(" + XMLHttpRequest.responseText + ")");
                        //alert(err.Message)
                        console.log("Ajax Error! " + err.Message);
                    }
                });
            },
            //select: function (e, i) {
            //    $("#txtSeaRICode").val(i.item.label);
            //    return false;
            //},
        });


        $('#btnSaveInsComp').on('click', function () {

            if ($.trim($('#InsurancecompanyName').val()) == "") {
                alert('Please enter InsurancecompanyName');
                $('#InsurancecompanyName').focus();
                return false;
            }
            if ($.trim($('#Address').val()) == "") {
                alert('Please enter Address');
                $('#Address').focus();
                return false;
            }
            if ($.trim($('#City').val()) == "") {
                alert('Please enter City');
                $('#City').focus();
                return false;
            }
            if ($.trim($('#State').val()) == "") {
                alert('Please enter State');
                $('#State').focus();
                return false;
            }
            if ($.trim($('#Zipcode').val()) == "") {
                alert('Please enter Zipcode');
                $('#Zipcode').focus();
                return false;
            }
            if ($.trim($('#ContactPerson').val()) == "") {
                alert('Please enter ContactPerson');
                $('#ContactPerson').focus();
                return false;
            }
            if ($.trim($('#MobileNumber').val()) == "") {
                alert('Please enter MobileNumber');
                $('#MobileNumber').focus();
                return false;
            }
            if ($.trim($('#EmailId').val()) == "") {
                alert('Please enter EmailId');
                $('#EmailId').focus();
                return false;
            }
            if ($.trim($('#BankName').val()) == "") {
                alert('Please enter BankName');
                $('#BankName').focus();
                return false;
            }
            if ($.trim($('#AccountNumber').val()) == "") {
                alert('Please enter AccountNumber');
                $('#AccountNumber').focus();
                return false;
            }
            if ($.trim($('#IFSCCode').val()) == "") {
                alert('Please enter IFSCCode');
                $('#IFSCCode').focus();
                return false;
            }
            /* if ($.trim($('#UploadKYC_01').val()) == "") {
                 alert('Please enter UploadKYC_01');
                 $('#UploadKYC_01').focus();
                 return false;
             }
             if ($.trim($('#UploadKYC_02').val()) == "") {
                 alert('Please enter UploadKYC_02');
                 $('#UploadKYC_02').focus();
                 return false;
             }
             if ($.trim($('#UploadKYC_03').val()) == "") {
                 alert('Please enter UploadKYC_03');
                 $('#UploadKYC_03').focus();
                 return false;
             }
             if ($.trim($('#UploadKYC_04').val()) == "") {
                 alert('Please enter UploadKYC_04');
                 $('#UploadKYC_04').focus();
                 return false;
             }
             if ($.trim($('#UploadKYC_05').val()) == "") {
                 alert('Please enter UploadKYC_05');
                 $('#UploadKYC_05').focus();
                 return false;
             }*/
            if ($.trim($('#BankName :selected').val()) == "") {
                alert("Please select BankName");
                $("#BankName").focus();
                return false;
            }

            if ($.trim($("#AccountNumber").val()) == "") {
                alert("Please enter Account Number");
                $("#AccountNumber").focus();
                return false;
            }
            if ($.trim($("#RepeatAccountNumber").val()) == "") {
                alert("Please enter Repeat Account Number");
                $("#RepeatAccountNumber").focus();
                return false;
            }
            if ($.trim($("#AccountNumber").val()) != "" && $.trim($("#RepeatAccountNumber").val()) != "") {
                if ($.trim($("#AccountNumber").val()) != $.trim($("#RepeatAccountNumber").val())) {
                    $("#RepeatAccountNumber").val("");
                    $("#RepeatAccountNumber").focus();
                    alert("Please check,Account Number is not match");
                    return false;
                }
            }

            if ($.trim($("#IFSCCode").val()) == "") {
                alert("Please enter IFSCCode");
                $("#IFSCCode").focus();
                return false;
            }

            var objCCMaster = new Object();
            objCCMaster.ICMId = $('#hdnICMasterID').val();
            objCCMaster.InsurancecompanyName = $('#InsurancecompanyName').val();
            objCCMaster.InsCompCode = $('#InsCompCode').val();
            objCCMaster.Address = $('#Address').val();
            objCCMaster.City = $('#City').val();
            objCCMaster.State = $('#State').val();
            objCCMaster.Zipcode = $('#Zipcode').val();
            objCCMaster.ContactPerson = $('#ContactPerson').val();
            objCCMaster.MobileNumber = $('#MobileNumber').val();
            objCCMaster.EmailId = $('#EmailId').val();
            objCCMaster.UploadKYC_01 = $('#UploadKYC_01').val();
            objCCMaster.UploadKYC_02 = $('#UploadKYC_02').val();
            objCCMaster.UploadKYC_03 = $('#UploadKYC_03').val();
            objCCMaster.UploadKYC_04 = $('#UploadKYC_04').val();
            objCCMaster.UploadKYC_05 = $('#UploadKYC_05').val();


            objCCMaster.BankName = $('#BankName :selected').text();
            objCCMaster.BankID = $('#BankName :selected').val();
            objCCMaster.AccountNumber = $("#AccountNumber").val();
            objCCMaster.IFSCCode = $("#IFSCCode").val();

            var param = { objReInsurerMaster: objCCMaster, Action: $("#btnSaveInsComp").val(), FolderName: $("#ICFolderName").val() };
            //var renewalData = $('#formRIMaster').serialize();
            $.ajax({
                type: "POST",
                url: "/GroupLifeInsurance/InsuranceCompanyMaster/AddUpdateCCMaster",
                dataType: "json",
                contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
                data: param,
                success: function (res) {
                    if (res > 0) {
                        Swal.fire({
                            icon: 'success',
                            title: '',
                            text: "Data Submitted Successfully.",
                        })
                    }
                    else if (res == -2) {
                        Swal.fire({
                            icon: 'warning',
                            title: '',
                            text: "Please provide all mandatory fields.",
                        })
                        //alert('Please provide all mandatory fields')
                        return false;
                    }
                    $('#formICMaster').reset();
                },
                error: function () {
                    // $("#lblMessage").html('Error while Registring the Company!.');
                    $('#formICMaster').reset();
                }
            })
        });
    });


    $('#btnSearch').on('click', function () {
        // var formData = $('#MotorForm').serialize();

        var param = { ICName: $('#txtCompanyName').val(), ICCode: $('#txtCompanyCode').val(), City: $('#txtCity').val() };
        $("#tblICMaster").find("tr:not(:first)").remove();

        $.ajax({

            type: "GET",
            url: "/GroupLifeInsurance/InsuranceCompanyMaster/GetICMaster",
            dataType: "json",
            contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
            data: param,
            success: function (Result) {
                if (Result.length > 0) {
                    var i = 0;
                    $.each(Result, function (key, value) {
                        i++;
                        var hdnID = "hdnobjJSonVal_" + i;
                        var JVal = JSON.stringify(value);
                        //$('#tblICMaster').append('<tr> <td>' + value.ReInsurerName + '</td>  <td>' + value.ReInsurerCode + '</td> <td>' + value.ContactPerson + '</td> <td>' + value.MobileNumber + '</td><td>' + value.EmailId + '</td> <td>' + value.Address + '</td><td>' + value.BankName + '</td><td>' + value.AccountNumber + '</td><td>' + value.IFSCCode + '</td><td>' + value.UploadKYC_01 + '</td><td class="text-center"><i class="mdi mdi-pencil text-primary mx-3" onclick="EditRIMaster(' + "'" + hdnID + "'" + ')"></i><i class="mdi mdi-delete text-danger mx-3" onclick="DeActiveICMaster(this,' + value.ReInsurerMasterId.toString() + ');"></i><input type="hidden" id=' + hdnID + ' value=' + "'" + JVal + "'" + '></td></tr>');
                        $('#tblICMaster').append('<tr> <td>' + value.InsurancecompanyName + '</td>  <td>' + value.InsCompCode + '</td> <td>' + value.ContactPerson + '</td> <td>' + value.MobileNumber + '</td><td>' + value.EmailId + '</td> <td>' + value.Address + '</td><td> <a href="' + value.KYCUploadPath + '" class="badge badge-pill badge-warning" download> Download KYC</a></td><td class="text-center"><i class="mdi mdi-pencil text-primary mx-3" onclick="EditICMaster(' + "'" + hdnID + "'" + ')"></i><i class="mdi mdi-delete text-danger mx-3" onclick="DeActiveICMaster(this,' + value.ICMId.toString() + ');"></i><input type="hidden" id=' + hdnID + ' value=' + "'" + JVal + "'" + '></td></tr>');

                    })
                }
                else {
                    $('#tblICMaster').append('<tr> <td colspan="14" aligen="center" > No Records found </td></tr>');
                }

            },
