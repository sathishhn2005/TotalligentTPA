        $.ajax({
            type: "POST",
            url: "/GroupLifeInsurance/ClientMaster/DeactivateCCMaster",
            dataType: "json",
            contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
            data: param,
            success: function (Result) {
                if (Result == "DeActivated") {
