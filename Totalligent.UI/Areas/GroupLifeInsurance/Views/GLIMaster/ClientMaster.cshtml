@using (Html.BeginForm("BulkUpdate", "ClientMaster", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div id="uploadBulk" class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-md">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title mt-0" id="uploadBulkLabel">Upload Bulk Policies</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-10 mx-auto">
                            <div class="text-right">

                                <a href="~/Masters/ClientMaster.csv" class="btn btn-dark btn-sm" download>
                                    <i class="mdi mdi-download"></i> Download Sample format
                                </a>
                            </div>
                            <div class="fileuploader fileuploader-theme-dragdrop">
                                <input type="file" name="CMCSVFile" id="fileUploadCM" accept=".csv" />
                                <div class="mt-2">
                                    <i class="fa fa-upload fa-sm btn btn-success btn-block" onclick="csvFileUpload();">
                                        <span>Upload</span>
                                    </i>
                                    <button id="uploadFiles" class="btn btn-primary btn-sm mx-1 hide">Upload File</button>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
            <!-- /.modal-content -->
        </div>
        <!-- /.modal-dialog -->
    </div>
}
<script type="text/javascript">
    var arr = [];
    $(document).ready(function () {

        $("#txtCompanyName").autocomplete({
            minLength: 2,
            autoFocus: true,
            open: function () { $($('.ui-autocomplete')).css('width', '300px'); },
            source: function (request, response) {
                var param = { prefixText: $('#txtCompanyName').val(), Action: "CCName" };

                $.ajax({
                    url: "/GroupLifeInsurance/ClientMaster/GetAutocompleteCCNameCCCodeCity",
                    data: JSON.stringify(param),
                    dataType: "json",
                    type: "POST",
                    contentType: "application/json; charset=utf-8",

                    success: function (data) {
                        response($.map(data, function (item) {
                            return {
                                label: item.ClientCompanyName,
                                value: item.ClientCompanyName
                            }
                        }))
                    },
                    error: function (XMLHttpRequest, textStatus, errorThrown) {
                        var err = eval("(" + XMLHttpRequest.responseText + ")");
                        //alert(err.Message)
                        console.log("Ajax Error!");
                    }
                });
            },
            //select: function (e, i) {
            //    $("#txtSeaRIMaster").val(i.item.label);
            //    return false;
            //},
        });

        $('#btnClientMaster').on('click', function () {

            if ($.trim($('#ClientCompanyName').val()) == "") {
                alert('Please enter ClientCompanyName');
                $('#ClientCompanyName').focus();
                return false;
            }

            if ($.trim($('#Address').val()) == "") {
                alert('Please enter Address');
                $('#Address').focus();
                return false;
            }
            if ($.trim($('#City').val()) == "") {
                alert('Please enter City');
                $('#City').focus();
                return false;
            }
            if ($.trim($('#State').val()) == "") {
                alert('Please enter State');
                $('#State').focus();
                return false;
            }
            if ($.trim($('#Zipcode').val()) == "") {
                alert('Please enter Zipcode');
                $('#Zipcode').focus();
                return false;
            }
            if ($.trim($('#ContactPerson').val()) == "") {
                alert('Please enter ContactPerson');
                $('#ContactPerson').focus();
                return false;
            }
            if ($.trim($('#MobileNumber').val()) == "") {
                alert('Please enter MobileNumber');
                $('#MobileNumber').focus();
                return false;
            }
            if ($.trim($('#EmailId').val()) == "") {
                alert('Please enter EmailId');
                $('#EmailId').focus();
                return false;
            }
            if ($.trim($('#Currency').val()) == "") {
                alert('Please enter Currency');
                $('#Currency').focus();
                return false;
            }
            if ($.trim($('#LOBName').val()) == "") {
                alert('Please enter LOB');
                $('#LOBName').focus();
                return false;
            }
            if ($.trim($('#ProducerType').val()) == "") {
                alert('Please enter ProducerType');
                $('#ProducerType').focus();
                return false;
            }
            if ($.trim($('#ProducerName').val()) == "") {
                alert('Please enter ProducerName');
                $('#Name').focus();
                return false;
            }
            if ($.trim($('#ProducerCommission').val()) == "") {
                alert('Please enter ProducerCommission');
                $('#ProducerCommission').focus();
                return false;
            }
            //if ($.trim($('#RINumber').val()) == "") {
            //    alert('Please enter RINumber');
            //    $('#RINumber').focus();
            //    return false;
            //}
            if ($.trim($('#RIName').val()) == "") {
                alert('Please enter RIName');
                $('#RIName').focus();
                return false;
            }
            if ($.trim($('#RIRetention').val()) == "") {
                alert('Please enter RIRetention');
                $('#RIRetention').focus();
                return false;
            }
            if ($.trim($('#AMIRetention').val()) == "") {
                alert('Please enter AMIRetention');
                $('#AMIRetention').focus();
                return false;
            }
            //if ($.trim($('#BankID').val()) == "") {
            //    alert('Please enter BankID');
            //    $('#BankID').focus();
            //    return false;
            //}
            if ($.trim($('#InsurancecompanyName').val()) == "") {
                alert('Please enter BankID');
                $('#BankID').focus();
                return false;
            }
            if ($.trim($('#AccountNumber').val()) == "") {
                alert('Please enter AccountNumber');
                $('#AccountNumber').focus();
                return false;
            }
            if ($.trim($('#IFSCCode').val()) == "") {
                alert('Please enter IFSCCode');
                $('#IFSCCode').focus();
                return false;
            }


            if ($.trim($("#AccountNumber").val()) != "" && $.trim($("#RepeatAccountNumber").val()) != "") {

                if ($.trim($("#AccountNumber").val()) != $.trim($("#RepeatAccountNumber").val())) {
                    $("#RepeatAccountNumber").val("");
                    $("#RepeatAccountNumber").focus();
                    alert("Please check,Account Number is not match");

                    return false;
                }

            }

            if ($.trim($("#IFSCCode").val()) == "") {
                alert("Please enter IFSCCode");
                $("#IFSCCode").focus();
                return false;
            }

            var objCCMaster = new Object();
            objCCMaster.ClientCompanyMasterId = $("#hdnCCMasterID").val();

            objCCMaster.ClientCompanyName = $('#ClientCompanyName').val();
            objCCMaster.InsuranceCompanyId = $('#InsurancecompanyName').val();
            objCCMaster.Address = $('#Address').val();
            objCCMaster.City = $('#City').val();
            objCCMaster.State = $('#State').val();
            objCCMaster.Zipcode = $('#Zipcode').val();
            objCCMaster.NationalityId = $('#NationalityId').val();
            objCCMaster.ContactPerson = $('#ContactPerson').val();
            objCCMaster.MobileNumber = $('#MobileNumber').val();
            objCCMaster.EmailId = $('#EmailId').val();
            objCCMaster.Currency = $('#Currency').val();
            objCCMaster.LOBName = $('#LOBName').val();
            objCCMaster.ProducerType = $('#ProducerType').val();
            objCCMaster.ProducerName = $('#ProducerName').val();
