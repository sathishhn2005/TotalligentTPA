    function GetUsers(BT) {
        if (BT == "") {
            $("#ProducerName").prop("disabled", true);
            $("#ProducerName").empty();
            return false;
        }
        var procemessage = "<option value='0'> Please wait...</option>";
        $("#ProducerName").html(procemessage).show();
        var url = "/GroupLifeInsurance/ClientMaster/GetProducerName";

        $.ajax({
            url: url,
            data: { BusinessType: BT },
            cache: false,
            type: "POST",
            success: function (data) {
                if (data.list != null) {
                    var lst = data.list
                    var markup = "<option value='0'>--Select Producer--</option>";
                    for (var x = 0; x < lst.length; x++) {
                        markup += "<option value=" + lst[x].ProducerMasterID + ">" + lst[x].ProducerName + "</option>";
                    }
                    $("#ProducerName").html(markup).show();
                    $("#ProducerName").prop("disabled", false);
