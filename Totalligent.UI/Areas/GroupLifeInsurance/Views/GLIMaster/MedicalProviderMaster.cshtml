    function DeActiveMPMaster(obj, RID) {

        var RIDs = '' + RID;
        var param = { MPId: RIDs };
        $.ajax({
            type: "POST",
            url: "/GroupLifeInsurance/MedicalProvider/DeactivateMPMaster",
            dataType: "json",
            contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
            data: param,
            success: function (Result) {
                if (Result == "DeActivated") {
                    $(obj).closest('tr').remove();
                    alert("DeActivated successfully");
                    return false;
                }
                else { alert(Result); }
            },
            error: function () {
                console.log('Error while deactivating!.');
            }
        })

    }
