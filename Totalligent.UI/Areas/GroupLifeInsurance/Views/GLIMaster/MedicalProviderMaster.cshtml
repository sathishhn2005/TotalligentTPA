<script type="text/javascript">

    //txtCitytxtMPCodetxtMPName
    $(document).ready(function () {

        $("#txtMP").autocomplete({
            minLength: 2,
            autoFocus: true,
            open: function () { $($('.ui-autocomplete')).css('width', '300px'); },
            source: function (request, response) {
                var param = { prefixText: $('#txtMP').val(), Action: "MedicalProvider" };

                $.ajax({
                    url: "/GroupLifeInsurance/MedicalProvider/GetAutocompleteMPNameProvider",
                    data: JSON.stringify(param),
                    dataType: "json",
                    type: "POST",
                    contentType: "application/json; charset=utf-8",

                    success: function (data) {
                        response($.map(data, function (item) {
                            return {
                                label: item.MedicalProvider,
                                value: item.MedicalProvider
                            }
                        }))
                    },
                    error: function (XMLHttpRequest, textStatus, errorThrown) {
                        var err = eval("(" + XMLHttpRequest.responseText + ")");
                        //alert(err.Message)
                        console.log("Ajax Error!");
                    }
                });
            },
            //select: function (e, i) {
            //    $("#txtSeaRIMaster").val(i.item.label);
            //    return false;
            //},
        });


        $('#btnSubmitMPMaster').on('click', function () {

            if ($.trim($('#MedicalProvider').val()) == "") {
                alert('Please enter MedicalProvider');
                $('#MedicalProvider').focus();
                return false;
            }
            if ($.trim($('#Name').val()) == "") {
                alert('Please enter Name');
                $('#Name').focus();
                return false;
            }

            if ($.trim($('#ContactPerson').val()) == "") {
                alert('Please enter ContactPerson');
                $('#ContactPerson').focus();
                return false;
            }
            if ($.trim($('#MobileNumber').val()) == "") {
                alert('Please enter MobileNumber');
                $('#MobileNumber').focus();
                return false;
            }
            if ($.trim($('#EmailId').val()) == "") {
                alert('Please enter EmailId');
                $('#EmailId').focus();
                return false;
            }
            if ($.trim($('#Address').val()) == "") {
                alert('Please enter Address');
                $('#Address').focus();
                return false;
            }
            if ($.trim($('#City').val()) == "") {
                alert('Please enter City');
                $('#City').focus();
                return false;
            }
            if ($.trim($('#State').val()) == "") {
                alert('Please enter State');
                $('#State').focus();
                return false;
            }
            if ($.trim($('#Zipcode').val()) == "") {
                alert('Please enter Zipcode');
                $('#Zipcode').focus();
                return false;
            }
            if ($.trim($('#Location').val()) == "") {
                alert('Please enter Location');
                $('#Location').focus();
                return false;
            }
            if ($.trim($('#Category').val()) == "") {
                alert('Please enter Category');
                $('#Category').focus();
                return false;
            }

            //if ($.trim($('#UploadKYC_01').val()) == "") {
            //    alert('Please enter UploadKYC_01');
            //    $('#UploadKYC_01').focus();
            //    return false;
            //}
            //if ($.trim($('#UploadKYC_02').val()) == "") {
            //    alert('Please enter UploadKYC_02');
            //    $('#UploadKYC_02').focus();
            //    return false;
            //}
            //if ($.trim($('#UploadKYC_03').val()) == "") {
            //    alert('Please enter UploadKYC_03');
            //    $('#UploadKYC_03').focus();
            //    return false;
            //}
            //if ($.trim($('#UploadKYC_04').val()) == "") {
            //    alert('Please enter UploadKYC_04');
            //    $('#UploadKYC_04').focus();
            //    return false;
            //}
            //if ($.trim($('#UploadKYC_05').val()) == "") {
            //    alert('Please enter UploadKYC_05');
            //    $('#UploadKYC_05').focus();
            //    return false;
            //}

            if ($.trim($('#BankName :selected').val()) == "") {
                alert("Please select BankName");
                $("#BankName").focus();
                return false;
            }

            if ($.trim($("#AccountNumber").val()) == "") {
                alert("Please enter Account Number");
                $("#AccountNumber").focus();
                return false;
            }
            if ($.trim($("#RepeatAccountNumber").val()) == "") {
                alert("Please enter Repeat Account Number");
                $("#RepeatAccountNumber").focus();
                return false;
            }
            if ($.trim($("#AccountNumber").val()) != "" && $.trim($("#RepeatAccountNumber").val()) != "") {
                if ($.trim($("#AccountNumber").val()) != $.trim($("#RepeatAccountNumber").val())) {
                    $("#RepeatAccountNumber").val("");
                    $("#RepeatAccountNumber").focus();
                    alert("Please check,Account Number is not match");
                    return false;
                }
            }

            if ($.trim($("#IFSCCode").val()) == "") {
                alert("Please enter IFSCCode");
                $("#IFSCCode").focus();
                return false;
            }

            var objMPMaster = new Object();
            objMPMaster.MedicalProviderId = $('#MedicalProviderId').val();
            objMPMaster.MedicalProvider = $('#MedicalProvider').val();
            objMPMaster.Name = $('#Name').val();
            objMPMaster.MedicalProviderCode = $('#MedicalProviderCode').val();
            objMPMaster.ContactPerson = $('#ContactPerson').val();
            objMPMaster.MobileNumber = $('#MobileNumber').val();
            objMPMaster.EmailId = $('#EmailId').val();
            objMPMaster.Address = $('#Address').val();
            objMPMaster.City = $('#City').val();
            objMPMaster.State = $('#State').val();
            objMPMaster.Zipcode = $('#Zipcode').val();
            objMPMaster.Location = $('#Location').val();
            objMPMaster.Category = $('#Category').val();

            objMPMaster.UploadKYC_01 = $('#UploadKYC_01').val();
            objMPMaster.UploadKYC_02 = $('#UploadKYC_02').val();
            objMPMaster.UploadKYC_03 = $('#UploadKYC_03').val();
            objMPMaster.UploadKYC_04 = $('#UploadKYC_04').val();
            objMPMaster.UploadKYC_05 = $('#UploadKYC_05').val();


            objMPMaster.BankName = $('#BankName :selected').text();
            objMPMaster.BankID = $('#BankName :selected').val();
            objMPMaster.AccountNumber = $("#AccountNumber").val();
            objMPMaster.IFSCCode = $("#IFSCCode").val();

            var param = { objMPMaster: objMPMaster, Action: $("#btnSubmitMPMaster").val() };
            //var renewalData = $('#formRIMaster').serialize();
            $.ajax({
                type: "POST",
                url: "/GroupLifeInsurance/MedicalProvider/AddUpdateMPMaster",
                dataType: "json",
                contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
                data: param,
                success: function (res) {
                    if (res > 0) {
                        Swal.fire({
                            icon: 'success',
                            title: '',
                            text: "Data Submitted Successfully.",
                        })
                    }
                    else if (res == -2) {
                        Swal.fire({
                            icon: 'warning',
                            title: '',
                            text: "Please provide all mandatory fields.",
                        })
                        //alert('Please provide all mandatory fields')
                        return false;
                    }
                    $('#formMPMaster').reset();
                },
                error: function () {
                    // $("#lblMessage").html('Error while Registring the Company!.');
                    $('#formMPMaster').reset();
                }
            })
        });
    });


    $('#btnSearch').on('click', function () {
        // var formData = $('#MotorForm').serialize();
        //txtContacttxtMPCode
        var param = { MedicalProvider: $('#txtMP').val(), Name: $('#txtName').val(), ContactPerson: $('#txtContactPerson').val() };
        $("#tblMPMaster").find("tr:not(:first)").remove();

        $.ajax({

            type: "GET",
            url: "/GroupLifeInsurance/MedicalProvider/GetMPMaster",
            dataType: "json",
            contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
            data: param,
            success: function (Result) {
                if (Result.length > 0) {
                    var i = 0;
                    $.each(Result, function (key, value) {
                        i++;
                        var hdnID = "hdnobjJSonVal_" + i;
                        var JVal = JSON.stringify(value);
