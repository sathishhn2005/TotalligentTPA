<script type="text/javascript">
    $("#VariableTypeSum").click(function () {

        if ($(this).is(":checked")) {
            var Gross = $("#Premium_VariableGrossPremium").val();
            $("#Premium_GrossPremium").val(Gross);
        }
        
    });
    $("#FixedTypeSum").click(function () {

        if ($(this).is(":checked")) {
            var Gross = $("#Premium_GrossPremium").val();
            $("#Premium_GrossPremium").val(Gross);
        }

    });
    function getReinsurerInfo() {
        if (!$('#objQuo_InsuranceCompanyName').val() > 0) {
            alert('Please Select Insurance Company');
            return false;
        }
        if (!$('#objQuo_ClientCompanyName').val() > 0) {
            alert('Please Select Client Company');
            return false;
        }
        var param = { InsuranceCompanyName: $('#objQuo_InsuranceCompanyName').val(), ClientCompanyName: $('#objQuo_ClientCompanyName option:selected').text()};
        $("#tblReinsurer").find("tr:not(:first)").remove();

        $.ajax({

            type: "GET",
            url: "/GroupLifeInsurance/Quotation/GetReInsurerRate",
            dataType: "json",
            contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
            data: param,
            success: function (Result) {
                if (Result.lstReinsurerRate.length > 0) {
                    //Result.lstReinsurerRate
                    //Result.objRIRateCalc
                    var FixedSA = Result.objRIRateCalc;
                    $('#Premium_SumAssured').val(FixedSA.Premium_SumAssured)
                    $('#Premium_GrossRate').val(FixedSA.Premium_GrossRate)
                    $('#Premium_NetPremium').val(FixedSA.Premium_NetPremium)
                    $('#Premium_Levy').val(FixedSA.Premium_Levy)
                    $('#Premium_PolicyFee1_RO').val(FixedSA.Premium_PolicyFee1RO)
                    $('#Premium_GrossPremium').val(FixedSA.Premium_GrossPremium)
                    $('#objQuo_Premium_BrokerName').val(FixedSA.Premium_BrokerName)
                    $('#objQuo_Premium_BrokerCommssion').val(FixedSA.Premium_BrokerCommssion)
                    $('#objQuo_Premium_Brokerage').val(FixedSA.Premium_Brokerage)

                    var KYCDetails = Result.objKYCDetails;
                    $('#objQuo_Address').val(KYCDetails.Address)
                    $('#objQuo_City').val(KYCDetails.City)
                    $('#objQuo_KYCDetails').val(KYCDetails.KYCUploadPath)
                    $('#objQuo_BankName').val(KYCDetails.BankName)
                    $('#objQuo_IFSCCode').val(KYCDetails.IFSCCode)

                    $("#hdnlstReinsurerRate").val(JSON.stringify(Result.lstReinsurerRate));
                    $.each(Result.lstReinsurerRate, function (key, value) {
                        $('#tblReinsurer').append('<tr> <td>' + value.Premium_RIName + '</td>  <td>' + value.Premium_RInetrate + '</td> <td>' + value.Premium_RIPremium + '</td> <td>' + value.Premium_RIRetention + '</td><td>' + value.Premium_InsurerRetention + '</td> <td>' + value.Premium_RIRetentionPremium + '</td> <td>' + value.Premium_InsurerRetentionPremium + '</td><td>' + value.Premium_InsurerLoadingPremium + '</td><td>' + value.Premium_InsurerLoading + '</td></tr>');
                    })
                }
                else {
                    $('#tblReinsurer').append('<tr> <td colspan="14" aligen="center" > No Records found </td></tr>');
                }

            },
            error: function () {
            }
        })
    }
    $(function () {
        $("#objQuo_WCId").change(function () {
            $('#divPerpersonRate input').val('');
            $('#divWCRate input').val('');
            $('#objQuo_Premium_Incl_levy_PolFeeRO').val('');
            $('#objQuo_GrossPremium').val('');
            $('#objQuo_NetPremium').val('');
            $('#objQuo_PolicyFee1_RO').val('');
            var selectedText = $(this).find("option:selected").text();
            var selectedValue = $(this).val();
            var ClientName = $('#objQuo_ClientCompanyName').val();
            if (ClientName > 0) {
                if (selectedValue == 1) {
                    GetWCRateCalc(ClientName);
                    $('#divWCRate').show();
                    $('#divPerpersonRate').hide();
                }
                else if (selectedValue == 2) {
                    GetPPRateCalc(ClientName);
                    $('#divWCRate').hide();
                    $('#divPerpersonRate').show();
                }
            }
            else {
                alert('Please Select Client.!');
                return false;
            }

        });
    });
    function GetWCRateCalc(ClientName) {

        var param = { ClientCompanyName: ClientName };
        $.ajax({

            type: "GET",
            url: "/GroupLifeInsurance/Quotation/GetWCRate",
            dataType: "json",
            contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
            data: param,
            success: function (Result) {

                $('#objQuo_SumOfSalary').val(Result.SumOfSalary);
                $('#objQuo_EstimatedAnnualWages').val(Result.EstimatedAnnualWages);
                $('#objQuo_Premium_Incl_levy_PolFeeRO').val(Result.Premium_Incl_levy_PolFeeRO);
                $('#objQuo_GrossPremium').val(Result.GrossPremium);
                $('#objQuo_NetPremium').val(Result.NetPremium);
                $('#objQuo_PolicyFee1_RO').val(Result.PolicyFee1_RO);
                $('#objQuo_WCRate').val(Result.WCRate);
                var WC_BrokerName = $('#objQuo_Premium_BrokerName').val();
                var WC_BrokerCommission = $('#objQuo_Premium_BrokerCommssion').val();
                var WC_NetPremium = Result.NetPremium;
                var WC_BrokerAmount = (parseFloat(WC_NetPremium) * parseFloat(WC_BrokerCommission)) / 100;
                $('#objQuo_BrokerName').val(WC_BrokerName);
                $('#objQuo_BrokerPercentage').val(WC_BrokerCommission);
                $('#objQuo_BrokerAmount').val(WC_BrokerAmount);
            },
            error: function () {
                // $("#lblMessage").html('Error while Registring the Company!.');
            }
        })
    }
