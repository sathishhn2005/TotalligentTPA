    function ReadDynamicControl() {

        var AdditionalCover = [];
        var Exclusion = [];
        //'DynAdditionalCoverClass' : 'DynExclusionsClass'
        var arrAdditionContent = $(".DynAdditionalCoverClass");
        var arrDynExclusions = $(".DynExclusionsClass");

        if (arrAdditionContent.length > 0) {
            for (var i = 0; i < arrAdditionContent.length; i++) {

                var id = arrAdditionContent[i].id;

                if (id.includes("dynTitle")) {

                    if ($(arrAdditionContent[i]).is(':checked')) {
                        item = {};
                        item["Title"] = $("#" + arrAdditionContent[i].id).val();
                        item["CoverageLimit"] = $.trim($("#" + arrAdditionContent[i].id.replace("dynTitle", "CoverageLimit")).val()) == "" ? "0" : $.trim($("#" + arrAdditionContent[i].id.replace("dynTitle", "CoverageLimit")).val());
