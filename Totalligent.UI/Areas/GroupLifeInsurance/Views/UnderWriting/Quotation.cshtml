<script type="text/javascript">
    function getReinsurerInfo() {
        if (!$('#objQuo_InsuranceCompanyName').val() > 0) {
            alert('Please Select Insurance Company');
            return false;
        }
        if (!$('#objQuo_ClientCompanyName').val() > 0) {
            alert('Please Select Client Company');
            return false;
        }
        var param = { InsuranceCompanyName: $('#objQuo_InsuranceCompanyName').val(), ClientCompanyName: $('#objQuo_ClientCompanyName option:selected').text()};
        $("#tblReinsurer").find("tr:not(:first)").remove();

        $.ajax({

            type: "GET",
            url: "/GroupLifeInsurance/Quotation/GetReInsurerRate",
            dataType: "json",
            contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
            data: param,
            success: function (Result) {
                if (Result.lstReinsurerRate.length > 0) {
                    //Result.lstReinsurerRate
                    //Result.objRIRateCalc
                    var FixedSA = Result.objRIRateCalc;
                    $('#Premium_SumAssured').val(FixedSA.Premium_SumAssured)
                    $('#Premium_GrossRate').val(FixedSA.Premium_GrossRate)
                    $('#Premium_NetPremium').val(FixedSA.Premium_NetPremium)
                    $('#Premium_Levy').val(FixedSA.Premium_Levy)
                    $('#Premium_PolicyFee1_RO').val(FixedSA.Premium_PolicyFee1RO)
                    $('#Premium_GrossPremium').val(FixedSA.Premium_GrossPremium)
                    $('#objQuo_Premium_BrokerName').val(FixedSA.Premium_BrokerName)
                    $('#objQuo_Premium_BrokerCommssion').val(FixedSA.Premium_BrokerCommssion)
                    $('#objQuo_Premium_Brokerage').val(FixedSA.Premium_Brokerage)

                    $.each(Result.lstReinsurerRate, function (key, value) {
                        $('#tblReinsurer').append('<tr> <td>' + value.Premium_RIName + '</td>  <td>' + value.Premium_RInetrate + '</td> <td>' + value.Premium_RIPremium + '</td> <td>' + value.Premium_RIRetention + '</td><td>' + value.Premium_InsurerRetention + '</td> <td>' + value.Premium_RIRetentionPremium + '</td> <td>' + value.Premium_InsurerRetentionPremium + '</td><td>' + value.Premium_InsurerLoadingPremium + '</td><td>' + value.Premium_InsurerLoading + '</td></tr>');
                    })
                }
                else {
                    $('#tblReinsurer').append('<tr> <td colspan="14" aligen="center" > No Records found </td></tr>');
                }

            },
            error: function () {
            }
        })
    }
    $(function () {
        $("#objQuo_WCId").change(function () {
            $('#divPerpersonRate input').val('');
            $('#divWCRate input').val('');
            $('#objQuo_Premium_Incl_levy_PolFeeRO').val('');
            $('#objQuo_GrossPremium').val('');
            $('#objQuo_NetPremium').val('');
