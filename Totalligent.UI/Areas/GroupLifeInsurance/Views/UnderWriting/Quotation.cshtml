            $("#PassiveWarRisk").click(function () {
                
                if ($(this).is(":checked")) {
                    
                    $("#PassiveWarRiskContent").slideDown();

                } else {
                    $("#PassiveWarRiskContent").slideUp();
                }
            });
            $("#HIV_AIDS").click(function () {
                
                if ($(this).is(":checked")) {
                    
                    $("#HIV_AIDSContent").slideDown();

                } else {
                    $("#HIV_AIDSContent").slideUp();
                }
            });
            $("#TerminalIllness").click(function () {
                
                if ($(this).is(":checked")) {
                    
                    $("#TerminalIllnessContent").slideDown();

                } else {
                    $("#TerminalIllnessContent").slideUp();
                }
            });
            $("#CriticalIllness").click(function () {
                
                if ($(this).is(":checked")) {
                    
                    $("#CriticalIllnessContent").slideDown();

                } else {
                    $("#CriticalIllnessContent").slideUp();
                }
            });
            $("input[name='DCGroup']").change(function () {
                
                if ($('#DAC:checked').val() === 'on') {
                    $("#DNCContent").slideUp();
                    $("#DACContent").slideDown();

                } else if ($('#DNC:checked').val() === 'on') {
                    $("#DNCContent").slideDown();
                    $("#DACContent").slideUp();
                }
            });
            $("input[name='DNCGroup']").change(function () {

                if ($('#FixedDNC:checked').val() === 'on') {
                    $("#FixedDNCContent").show();
                    $("#VariableDNCContent").hide();
                    $("#DNCFCLContent").show();

                } else if ($('#VariableDNC:checked').val() === 'on') {
                    $("#FixedDNCContent").hide();
                    $("#VariableDNCContent").show();
                    $("#DNCFCLContent").show();
                }
            });
            $("input[name='DACGroup']").change(function () {

                if ($('#FixedDAC:checked').val() === 'on') {
                    $("#FixedDACContent").show();
                    $("#VariableDACContent").hide();
                    $("#DACFCLContent").show();

                } else if ($('#VariableDAC:checked').val() === 'on') {
                    $("#FixedDACContent").hide();
                    $("#VariableDACContent").show();
                    $("#DACFCLContent").show();
                }
            });
            $("input[name='ACDGroup']").change(function () {

                if ($('#FixedACD:checked').val() === 'on') {
                    $("#FixedACDContent").show();
                    $("#VariableACDContent").hide();
                    $("#ACDFCLContent").show();

                } else if ($('#VariableACD:checked').val() === 'on') {
                    $("#FixedACDContent").hide();
                    $("#VariableACDContent").show();
                    $("#ACDFCLContent").show();
                }
            });
            $("input[name='TypeSum']").change(function () {

                if ($('#FixedTypeSum:checked').val() === 'on') {
                    $("#FixedTypeSumContent").show();
                    $("#VariableTypeSumContent").hide();

                } else if ($('#VariableTypeSum:checked').val() === 'on') {
                    $("#FixedTypeSumContent").hide();
                    $("#VariableTypeSumContent").show();

                }
            });
        });
        
    };


    function initVerticalWizard() {
        $("#example-vertical").steps({
            headerTag: "h3",
            bodyTag: "div",
            transitionEffect: "none",
            titleTemplate: '#title#',
            stepsOrientation: "vertical"
        });
    }

    function initStepWizard() {
        //$('[data-layout="sidebar-collapse"]').click();
        $(".tab-wizard").steps({
            headerTag: "h6",
            bodyTag: "section",
            transitionEffect: "none",
            titleTemplate: '#title#',

            labels: {
                finish: "Submit"
            },
            onFinished: function (event, currentIndex) {
                //swal("Your Order Submitted!", "Sed dignissim lacinia nunc. Curabitur tortor. Pellentesque nibh. Aenean quam. In scelerisque sem at dolor. Maecenas mattis. Sed convallis tristique sem. Proin ut ligula vel nunc egestas porttitor.");

                var quotationData = $('#formQuotation').serialize();
                $.ajax({

                    type: "POST",
                    url: "/Quotation/QuotationMaster",
                    dataType: "json",
                    contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
                    data: quotationData,
                    success: function (res) {
                        if (res.length > 0) {
                            //Swal.fire({
                            //    icon: 'success',
                            //    title: '',
                            //    text: "Data Submitted Successfully.   Draft No is '" + res + "'",
                            //})
                            alert('Data Submitted Successfully');
                        }

                        // $('#formRenewal').reset();
                    },
                    error: function () {
                        alert('Data Submitted Successfully');
                        // $("#lblMessage").html('Error while Registring the Company!.');
                        $('#formRenewal').reset();
                    }
                })


            }

        });
    }
    function exclusionsfn(checkBoxId, checkBoxContent) {
        if ($('#' + checkBoxId).is(":checked")) {

            $("#" + checkBoxContent).slideDown();

        } else {
            $("#" + checkBoxContent).slideUp();
        }
    }
</script>