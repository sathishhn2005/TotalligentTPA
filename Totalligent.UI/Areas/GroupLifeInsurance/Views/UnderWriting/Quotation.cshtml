    function UpdateVariableSumAssured(obj, paramCatName) {

        var arrVal = JSON.parse($("#hdnVariableTypeSumContent").val());
        var TotNoOfTimes = 0, TotSalary = 0, TotSA = 0, TotClientRate = 0; TotNetPremium = 0;

        $.each(arrVal, function (i, value) {
            if ($.trim(value.CategoryName) == $.trim(paramCatName)) {
                if (obj.name == "Salary") {
                    arrVal[i].Salary = obj.value == "" ? "0" : obj.value;
                }
                if (obj.name == "NoOfTimes") {
                    arrVal[i].NoOfTimes = obj.value == "" ? "0" : obj.value;
                    arrVal[i].SumAssured = obj.value == "" ? "0" : arrVal[i].Salary * arrVal[i].NoOfTimes;
                    $("#txtSumAssured_" + i).val(arrVal[i].SumAssured);
                }
                if (obj.name == "ClientRate") {
                    arrVal[i].ClientRate = obj.value == "" ? "0" : obj.value;
                    arrVal[i].NetPremium = obj.value == "" ? "0" : (arrVal[i].ClientRate * arrVal[i].SumAssured) / 100;;
                    $("#txtNetPremium_" + i).val(arrVal[i].NetPremium);

                }
                if (obj.name == "NetPremium") {
                    arrVal[i].NetPremium = obj.value == "" ? "0" : obj.value;
                }
            }

            if (obj.name == "NoOfTimes") {
                TotNoOfTimes = TotNoOfTimes + Number(arrVal[i].NoOfTimes);
                $("#lblNoOfTimes").text("OMR " + TotNoOfTimes);
                TotSA = TotSA + Number(arrVal[i].SumAssured);
                $("#lbltotalSA").text("OMR " + TotSA);

            }
            if (obj.name == "Salary") {
                TotSalary = TotSalary + Number(arrVal[i].Salary);
                $("#lblTotSalary").text("OMR " + TotSalary);
            }

            if (obj.name == "ClientRate") {
                TotClientRate = TotClientRate + Number(arrVal[i].ClientRate);
                $("#lblClientRate").text(TotClientRate.toFixed(3));
                TotNetPremium = TotNetPremium + (Number(arrVal[i].ClientRate) * Number(arrVal[i].SumAssured) / 100);

                $("#lbltotalNetPremium").text(TotNetPremium.toFixed(3));
                $("#Premium_VariableNetPremium").val(TotNetPremium.toFixed(3));

                var Premium_VariableLevy = (TotNetPremium * Number(0.55)) / 100;
                $("#Premium_VariableLevy").val(Premium_VariableLevy.toFixed(3));


                var Premium_VariablePolicyFee1_RO = 1;
                $("#Premium_VariablePolicyFee1_RO").val(Premium_VariablePolicyFee1_RO.toFixed(3));


                var Premium_VariableGrossPremium = Number(TotNetPremium) + Number(Premium_VariableLevy) + Number(Premium_VariablePolicyFee1_RO);
                $("#Premium_VariableGrossPremium").val(Premium_VariableGrossPremium.toFixed(3));

                //objRIRate.Premium_Levy = (objRIRate.Premium_NetPremium * Convert.ToDecimal(Premium_Levy)) / 100;
                //objRIRate.Premium_PolicyFee1RO = 1;
                //objRIRate.Premium_GrossPremium = objRIRate.Premium_NetPremium + objRIRate.Premium_Levy + objRIRate.Premium_PolicyFee1RO;
            }


        });



        $("#hdnVariableTypeSumContent").val(JSON.stringify(arrVal));
        //alert(obj.value, paramCatName);
    }

