    function PolicyIssuancefn() {
        //$('[data-layout="sidebar-collapse"]').click();
        $(".tab-wizard").steps({
            headerTag: "h6",
            bodyTag: "section",
            transitionEffect: "none",
            titleTemplate: '#title#',
            enableCancelButton: true,
            labels: {
                finish: "Approve",
                cancel: "Reject"
            },
            onFinished: function (event, currentIndex) {
                //swal("Your Order Submitted!", "Sed dignissim lacinia nunc. Curabitur tortor. Pellentesque nibh. Aenean quam. In scelerisque sem at dolor. Maecenas mattis. Sed convallis tristique sem. Proin ut ligula vel nunc egestas porttitor.");

                var quotationData = $('#formQuotation').serialize();

                $.ajax({

                    type: "POST",
                    url: "/GroupLifeInsurance/Quotation/AddUpdateQuotationMaster",
                    dataType: "json",
                    contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
                    data: quotationData,
                    success: function (res) {
                        if (res.QuotationId > 0) {
                            console.log(res);
                            Swal.fire({
                                icon: 'success',
                                title: '',
                                text: "'" + res.Status + "'/n Policy No is '" + res.PolicyNo + "'/n WLPolicy No Is '" + res.WLPolicyNo + "'",
                            });

                        }
                        else {
                            Swal.fire({
                                icon: 'success',
                                title: '',
                                text: "'" + res.Status + "'/n Draft No is '" + res.DraftNo + "'/n WLDraft No Is '" + res.WLDraftNo + "'",
                            });
                        }

                        window.location = '@Url.Action("Quotation", "UnderWriting")'
                       // $('#formQuotation').reset();
                    },
                    error: function () {
                       window.location = '@Url.Action("Quotation", "UnderWriting")'
                    }
                })


            },
            onCanceled: function (event, currentIndex) {

                var quotationData = $('#formQuotation').serialize();
                var param = { Qid: $('#objQuo_QuotationId').val(), Action: "Reject" };
                $.ajax({

                    type: "POST",
                    url: "/GroupLifeInsurance/Quotation/RejectQuotation",
                    dataType: "json",
                    contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
                    data: param,
                    success: function (res) {
                        alert(res);
                       window.location = '@Url.Action("Quotation", "UnderWriting")'

                    },
                    error: function () {
                        alert('Error while rejecting the draft');
                        window.location = '@Url.Action("Quotation", "UnderWriting")'

                        // $("#lblMessage").html('Error while Registring the Company!.');
                        $('#formQuotation').reset();
                    }
                });
            }

        });
    }
