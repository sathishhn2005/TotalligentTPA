
@model Totalligent.UI.Models.QuotationModel
@using Totalligent.BusinessEntities;

@{
    ViewBag.Title = "Masters";
    Layout = "~/Areas/GroupLifeInsurance/Views/Shared/_Layout.cshtml";
}
@*@Scripts.Render("~/Content/vendor/jquery-3.3.1/jquery-3.3.1.js");*@
<div class="row">
    <div class="col-md-12 p-t-30">
        <!-- Step wizard -->
        <div class="box box-default">
            <!-- /.box-header -->
            <div class="box-body wizard-content">
                <form id="formQuotation" action="#" class="tab-wizard wizard-circle">
                    <input type="hidden" id="hdnQid" name="QuotationId" value="0" />
                    <input type="hidden" id="objQuoJson" value="@Model.objQuoJson" />

                    <!-- Step 1 -->
                    <h6>Client Information</h6>
                    <section>
                        <div class="row">
                            <div class="hide">

                                @if (Model != null)
                                {
                                    @Html.TextBoxFor(model => model.objQuo.QuotationId, new { @class = "form-control w-full", @placeholder = "Enter Scope of Coverage" })
                                    @Html.TextBoxFor(model => model.objQuo.Status, new { @class = "form-control w-full", @placeholder = "Enter Scope of Coverage" })
                                }
                            </div>
                            <div class="col-md-10 m-b-15 m-t-15 offset-md-1">
                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-4">
                                        <label class="col-sm-12 col-form-label text-left">Insurance Company Name</label>
                                    </div>
                                    @*$("#LifeEmgAreaCoverage").data("kendoMultiSelect").dataItems()*@

                                    <div class="col-md-8 demo-section k-content">
                                        @Html.DropDownListFor(model => model.objQuo.InsuranceCompanyName, Model.lstInsuranceCompanyMaster, "-- Select Type --", new { @class = "form-control" })
                                    </div>
                                </div>
                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-4">
                                        <label class="col-sm-12 col-form-label text-left">Client Company Name</label>
                                    </div>
                                    <div class="col-md-8 demo-section k-content">
                                        @Html.DropDownListFor(model => model.objQuo.ClientCompanyName, Model.lstClientMaster, "-- Select Type --", new { @onchange = "getReinsurerInfo()", @class = "form-control" })

                                    </div>
                                </div>
                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-4">
                                        <label class="col-sm-12 col-form-label text-left">Area of Coverage</label>
                                    </div>
                                    <div class="col-md-8 demo-section k-content">
                                        @Html.TextBox("AreaCoverage", "", new { @class = "form-control w-full readonly", @Value = "Worldwide", @readonly = "readonly" })
                                        @*@Html.TextBoxFor(model => model.objQuo.ScopeofCoverage, new { @class = "form-control w-full", @placeholder = "Enter Scope of Coverage" })*@

                                    </div>
                                </div>
                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-4">
                                        <label class="col-sm-12 col-form-label text-left">Jurisdiction</label>
                                    </div>
                                    <div class="col-md-8 demo-section k-content">
                                        @Html.TextBox("Jurisdiction", "", new { @class = "form-control w-full", @Value = "SULTANATE OF OMAN", @readonly = "readonly" })
                                        @*@Html.TextBoxFor(model => model.objQuo.Jurisdiction, new { @class = "form-control w-full", @placeholder = "Enter Jurisdiction" })*@
                                    </div>
                                </div>
                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-4">
                                        <label class="col-sm-12 col-form-label text-left">Address</label>
                                    </div>
                                    <div class="col-md-8 demo-section k-content">
                                        @Html.TextBoxFor(model => model.objQuo.Address, new { @class = "form-control w-full", @placeholder = "Enter Address" })
                                    </div>
                                </div>

                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-4">
                                        <label class="col-sm-12 col-form-label text-left">City</label>
                                    </div>
                                    <div class="col-md-8 demo-section k-content">
                                        @Html.TextBoxFor(model => model.objQuo.City, new { @class = "form-control w-full", @placeholder = "Enter City" })
                                    </div>
                                </div>
                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-4">
                                        <label class="col-sm-12 col-form-label text-left">KYC Details</label>
                                    </div>
                                    <div class="col-md-8 demo-section k-content">
                                        @Html.TextBoxFor(model => model.objQuo.KYCDetails, new { @class = "form-control w-full", @placeholder = "Enter KYCDetails" })
                                    </div>
                                </div>
                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-4">
                                        <label class="col-sm-12 col-form-label text-left">Bank Name</label>
                                    </div>
                                    <div class="col-md-8 demo-section k-content">
                                        @Html.TextBoxFor(model => model.objQuo.BankName, new { @class = "form-control w-full", @placeholder = "Enter BankName" })
                                    </div>
                                </div>

                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-4">
                                        <label class="col-sm-12 col-form-label text-left">IFSC Code</label>
                                    </div>
                                    <div class="col-md-8 demo-section k-content">
                                        @Html.TextBoxFor(model => model.objQuo.IFSCCode, new { @class = "form-control w-full", @placeholder = "Enter IFSCCode" })
                                    </div>
                                </div>
                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-4">
                                        <label class="col-sm-12 col-form-label text-left">Branch</label>
                                    </div>
                                    <div class="col-md-8 demo-section k-content">
                                        @*@Html.TextBoxFor(model => model.objQuo.Branch, new { @class = "form-control w-full", @placeholder = "Enter Branch" })*@
                                        @Html.DropDownListFor(model => model.objQuo.Branch, Model.lstBranchMaster, "-- Select Type --", new { @class = "form-control" })
                                    </div>
                                </div>
                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-4">
                                        <label class="col-sm-12 col-form-label text-left">Period of Insurance</label>
                                    </div>
                                    <div class="col-md-8 demo-section k-content">
                                        @Html.TextBoxFor(model => model.objQuo.PeriodofInsurance, "{0:yyyy-MM-dd}", new { @type = "date", @class = "form-control w-full", @placeholder = "Enter PeriodofInsurance" })
                                    </div>
                                </div>
                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-4">
                                        <label class="col-sm-12 col-form-label text-left">Category</label>
                                    </div>
                                    <div class="col-md-8  ">
                                        <div class="row">
                                            <div class="col-md-12 d-flex">

                                                @Html.TextBoxFor(model => model.objQuo.Category, new { @id = "CategoryName", @class = "form-control w-full", @placeholder = "Enter Category" })
                                                <button type="button" onclick="bindCategoryType('addCategory');" class="btn btn-primary ml-3"><i class="mdi mdi-check"></i></button>
                                            </div>
                                            <div class="col-md-12">

                                                <div class="table-responsive" id="divCategoryType">



                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
                    <!-- Step 2 -->
                    <h6>Coverage Details</h6>
                    <section>
                        <div class="row">
                            <div class="col-12">
                                <hr />
                                <h5 class="m-b-15 m-t-15 text-bold col-12 p-0">Death Cover</h5>
                                <hr />
                                <div class="col-md-11 m-b-15 m-t-15 mx-auto">
                                    <div class="row m-b-15 m-t-15">
                                        <div class="col-md-3">
                                            @*@Html.RadioButtonFor(model => model.objQuo.DeathCoverType, "DAC" ,new { @id = "DAC", @onclick = "AssignDeathCoverValues('DAC');", @class = "with-gap radio-col-primary",@text= "Death due to any cause (DAC)", @placeholder = "Enter Category" })*@
                                            <input name="DeathCoverType" type="radio" value="DAC" id="DAC" onclick="AssignDeathCoverValues('DAC');" class="with-gap radio-col-primary" />
                                            <label for="DAC">Death due to any cause (DAC)</label>
                                        </div>
                                        <div class="col-md-7 demo-section k-content">
                                            <div id="DACContent" style="display: none;">
                                                <div class="d-flex">
                                                    <label class="text-left font-weight-bold col-3 pl-1">Annual Limit</label>
                                                    <div class="demo-radio-button d-flex justify-content-between">
                                                        <span>
                                                            <input name="DeathCoverAnnualLimit" type="radio" value="Fixed" id="FixedDAC" class="with-gap radio-col-primary" onclick="AssignDeathCoverValues('DACFixed');" />
                                                            <label for="FixedDAC">Fixed</label>
                                                        </span>
                                                        <span>
                                                            <input name="DeathCoverAnnualLimit" type="radio" value="Variable" id="VariableDAC" class="with-gap radio-col-primary" onclick="AssignDeathCoverValues('DACVariable');" />
                                                            <label for="VariableDAC">Variable</label>
                                                        </span>
                                                    </div>
                                                </div>

                                                <div class="row m-b-15 m-t-15" id="FixedDACContent" style="display: none;">
                                                    <div class="col-md-5">
                                                        <label class="col-sm-12 col-form-label text-left pl-1">Sum Assured</label>
                                                    </div>
                                                    <div class="col-md-7 input-group">
                                                        <span class="input-group-prepend">
                                                            <span class="input-group-text">OMR</span>
                                                        </span>
                                                        <input type="number" value="" id="FixedDCSAmount" class="form-control" placeholder="Enter Sum Assured Amount" onchange="AssignDeathCoverValues('DACtxt');">
                                                        <input type="hidden" id="hdnDeathCoverSumAssuredAmount" name="DeathCoverSumAssuredAmount" value="" />
                                                        <input type="hidden" id="hdnDeathCoverFCL" name="DeathCoverFCL" value="" />
                                                        <input type="hidden" id="hdnDeathCoverSumAssuredNoOfTimes" name="DeathCoverSumAssuredNoOfTimes" value="" />
                                                    </div>
                                                </div>
                                                <div class="row m-b-15 m-t-15" id="VariableDACContent" style="display: none;">
                                                    <div class="col-md-5">
                                                        <label class="col-sm-12 col-form-label text-left pl-1">Sum Assured</label>
                                                    </div>
                                                    <div class="col-md-7 input-group">
                                                        <span class="input-group-prepend">
                                                            <span class="input-group-text">OMR</span>
                                                        </span>
                                                        <input type="number" id="VariableDCSAmount" class="form-control" placeholder="Enter Sum Assured Amount" onchange="AssignDeathCoverValues('DACtxt');">
                                                        <span class="input-group-text group-text-custom">*</span>
                                                        <input type="number" id="txtDeathCoverSumAssuredNoOfTimes" class="form-control" placeholder="Enter No.of times" onchange="AssignDeathCoverValues('DACtxt');">
                                                    </div>
                                                </div>
                                                <div class="row m-b-15 m-t-15" id="DACFCLContent" style="display: none;">
                                                    <div class="col-md-5">
                                                        <label class="col-sm-12 col-form-label text-left pl-1">FCL</label>
                                                    </div>
                                                    <div class="col-md-7">
                                                        <input type="number" id="txtDeathCoverFCL" onchange="AssignDeathCoverValues('DACtxt');" class="form-control" placeholder="Enter FCL">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row m-b-15 m-t-15">
                                        <div class="col-md-3">
                                            <input name="DeathCoverType" value="DNC" onclick="AssignDeathCoverValues('DNC');" type="radio" id="DNC" class="with-gap radio-col-primary" />
                                            <label for="DNC">Death by natural cause (DNC)</label>
                                        </div>
                                        <div class="col-md-7 demo-section k-content">
                                            <div id="DNCContent" style="display: none;">
                                                <div class="d-flex">
                                                    <label class="text-left font-weight-bold col-3 pl-1">Annual Limit</label>
                                                    <div class="demo-radio-button d-flex justify-content-between">
                                                        <span>
                                                            <input name="DNCGroup" type="radio" value="Fixed" id="FixedDNC" onclick="AssignDeathCoverValues('FixedDNC')" class="with-gap radio-col-primary" />
                                                            <label for="FixedDNC">Fixed</label>
                                                        </span>
                                                        <span>
                                                            <input name="DNCGroup" type="radio" value="Variable" id="VariableDNC" onclick="AssignDeathCoverValues('VariableDNC')" class="with-gap radio-col-primary" />
                                                            <label for="VariableDNC">Variable</label>
                                                        </span>
                                                    </div>
                                                </div>

                                                <div class="row m-b-15 m-t-15" id="FixedDNCContent" style="display: none;">
                                                    <div class="col-md-5">
                                                        <label class="col-sm-12 col-form-label text-left pl-1">Sum Assured</label>
                                                    </div>
                                                    <div class="col-md-7 input-group">
                                                        <span class="input-group-prepend">
                                                            <span class="input-group-text">OMR</span>
                                                        </span>
                                                        <input type="text" id="txtDNCSAA" class="form-control" placeholder="Enter Sum Assured Amount" onchange="AssignDeathCoverValues('DNCtxt');">
                                                    </div>
                                                </div>
                                                <div class="row m-b-15 m-t-15" id="VariableDNCContent" style="display: none;">
                                                    <div class="col-md-5">
                                                        <label class="col-sm-12 col-form-label text-left pl-1">Sum Assured</label>
                                                    </div>
                                                    <div class="col-md-7 input-group">
                                                        <span class="input-group-prepend">
                                                            <span class="input-group-text">OMR</span>
                                                        </span>
                                                        <input type="text" class="form-control" id="txtDNCSAA01" placeholder="Enter Sum Assured Amount" onchange="AssignDeathCoverValues('DNCtxt');">
                                                        <span class="input-group-text group-text-custom">*</span>
                                                        <input type="text" id="txtEnterNooftimes" class="form-control" placeholder="Enter No.of times" onchange="AssignDeathCoverValues('DNCtxt');">

                                                    </div>
                                                </div>
                                                <div class="row m-b-15 m-t-15" id="DNCFCLContent" style="display: none;">
                                                    <div class="col-md-5">
                                                        <label class="col-sm-12 col-form-label text-left pl-1">FCL</label>
                                                    </div>
                                                    <div class="col-md-7">
                                                        <input type="number" id="txtDNCFCLContent" class="form-control" placeholder="Enter FCL" onchange="AssignDeathCoverValues('DNCtxt');">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row m-b-15 m-t-15">
                                        <div class="col-md-3">
                                            <input type="checkbox" id="AccidentalDeath" value="Y" name="AccidentalDeath" />
                                            <label for="AccidentalDeath">Accidental death</label>
                                        </div>
                                        <div class="col-md-7 demo-section k-content">
                                            <div id="ACDContent" style="display: none;">
                                                <div class="d-flex">
                                                    <label class="text-left font-weight-bold col-3 pl-1">Annual Limit</label>
                                                    <div class="demo-radio-button d-flex justify-content-between">
                                                        <span>
                                                            <input name="AccidentalAnnualLimit" value="Fixed" type="radio" id="FixedACD" onclick="AssignDeathCoverValues('FixedADC');" class="with-gap radio-col-primary" />
                                                            <label for="FixedACD">Fixed</label>

                                                        </span>
                                                        <span>
                                                            <input name="AccidentalAnnualLimit" type="radio" value="Variable" id="VariableACD" onclick="AssignDeathCoverValues('VariableADC');" class="with-gap radio-col-primary" />
                                                            <label for="VariableACD">Variable</label>
                                                        </span>
                                                    </div>
                                                </div>

                                                <div class="row m-b-15 m-t-15" id="FixedACDContent" style="display: none;">
                                                    <div class="col-md-5">
                                                        <label class="col-sm-12 col-form-label text-left pl-1">Sum Assured</label>
                                                    </div>
                                                    <div class="col-md-7 input-group">
                                                        <span class="input-group-prepend">
                                                            <span class="input-group-text">OMR</span>
                                                        </span>
                                                        <input type="number" id="txtAccidentalDeathSAM" onchange="AssignDeathCoverValues('txtACD');" class="form-control" placeholder="Enter Sum Assured Amount">
                                                        <input type="hidden" id="hdnAccidentalSumAssuredAmount" name="AccidentalSumAssuredAmount" value="" />

                                                    </div>
                                                </div>
                                                <div class="row m-b-15 m-t-15" id="VariableACDContent" style="display: none;">
                                                    <div class="col-md-5">
                                                        <label class="col-sm-12 col-form-label text-left pl-1">Sum Assured</label>
                                                    </div>
                                                    <div class="col-md-7 input-group">
                                                        <span class="input-group-prepend">
                                                            <span class="input-group-text">OMR</span>
                                                        </span>
                                                        <input type="number" id="txtACDSAM" onchange="AssignDeathCoverValues('txtACD');" class="form-control" placeholder="Enter Sum Assured Amount">
                                                        <span class="input-group-text group-text-custom">*</span>
                                                        <input type="number" id="txtACDNoOfTimes" name="AccidentalSumAssuredNoOfTimes" class="form-control" placeholder="Enter No.of times">
                                                    </div>
                                                </div>
                                                <div class="row m-b-15 m-t-15" id="ACDFCLContent" style="display: none;">
                                                    <div class="col-md-5">
                                                        <label class="col-sm-12 col-form-label text-left pl-1">FCL</label>
                                                    </div>
                                                    <div class="col-md-7">
                                                        <input type="number" id="txtAccidentalFCL" name="AccidentalFCL" class="form-control" placeholder="Enter FCL">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12">
                                <hr />
                                <h5 class="m-b-15 m-t-15 text-bold col-12 p-0">Benefits</h5>
                                <hr />
                                <div class="col-md-11 m-b-15 m-t-15 mx-auto">
                                    <div class="row m-b-15 m-t-15">
                                        <div class="col-md-3">
                                            <input type="checkbox" id="PermanentDisability" value="Y" name="PTD" />
                                            <label for="PermanentDisability">Permanent Total Disability (PTD)</label>
                                        </div>
                                        <div class="col-md-7 demo-section k-content">
                                            <div id="PermanentDisabilityContent" style="display: none;">
                                                <div class="demo-radio-button d-flex justify-content-start">
                                                    <span>
                                                        <input type="checkbox" id="AccidentPTD" name="PTDAccident" value="Y" />
                                                        <label for="AccidentPTD">Accident</label>
                                                    </span>
                                                    <span>
                                                        <input type="checkbox" id="SicknessPTD" name="PTDSickness" value="Y" />
                                                        <label for="SicknessPTD">Sickness</label>
                                                    </span>
                                                </div>
                                                <div class="row m-b-15 m-t-15">
                                                    <div class="col-md-5">
                                                        <label class="col-sm-12 col-form-label text-left pl-1">Sum Assured</label>
                                                    </div>
                                                    <div class="col-md-7 input-group">
                                                        <span class="input-group-prepend">
                                                            <span class="input-group-text">OMR</span>
                                                        </span>
                                                        <input type="number" class="form-control" id="txtPTDCoverageLimit" name="PTDCoverageLimit" placeholder="Enter Sum Assured">
                                                    </div>
                                                </div>
                                                <div class="row m-b-15 m-t-15 d-none">
                                                    <div class="col-md-5">
                                                        <label class="col-sm-12 col-form-label text-left pl-1">Maximum Limit</label>
                                                    </div>
                                                    <div class="col-md-7 input-group">
                                                        <span class="input-group-prepend">
                                                            <span class="input-group-text">OMR</span>
                                                        </span>
                                                        <input type="number" class="form-control" id="txtPTDMaximumLimit" name="PTDMaximumLimit" placeholder="Enter Maximum Amount">
                                                    </div>
                                                </div>
                                                <div class="row m-b-15 m-t-15">
                                                    <div class="col-md-5">
                                                        <label class="col-sm-12 col-form-label text-left pl-1">Waiting Period in Days</label>
                                                    </div>
                                                    <div class="col-md-7 input-group">

                                                        <input type="number" class="form-control" id="txtPTDWaitingYears" name="PTDWaitingYears" placeholder="Enter Coverage Amount">
                                                        <span class="input-group-append">
                                                            <span class="input-group-text">Days</span>
                                                        </span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row m-b-15 m-t-15">
                                        <div class="col-md-3">
                                            <input type="checkbox" id="PermanentPartialDisability" name="PPD" value="Y" />
                                            <label for="PermanentPartialDisability">Permanent Partial Disability (PPD)</label>
                                        </div>
                                        <div class="col-md-7 demo-section k-content">
                                            <div id="PermanentPartialDisabilityContent" style="display: none;">
                                                <div class="demo-radio-button d-flex justify-content-start">
                                                    <span>
                                                        <input type="checkbox" id="AccidentPPD" name="PPDAccident" value="Y" />
                                                        <label for="AccidentPPD">Accident</label>
                                                    </span>
                                                    <span>
                                                        <input type="checkbox" id="SicknessPPD" name="PPDSickness" value="Y" />
                                                        <label for="SicknessPPD">Sickness</label>
                                                    </span>
                                                </div>
                                                <div class="row m-b-15 m-t-15">
                                                    <div class="col-md-5">
                                                        <label class="col-sm-12 col-form-label text-left pl-1">Sum Assured</label>
                                                    </div>
                                                    <div class="col-md-7 input-group">
                                                        <span class="input-group-prepend">
                                                            <span class="input-group-text">OMR</span>
                                                        </span>
                                                        <input type="number" class="form-control" id="txtPPDCoverageLimit" name="PPDCoverageLimit" placeholder="Enter Sum Assured">
                                                    </div>
                                                </div>
                                                <div class="row m-b-15 m-t-15 d-none">
                                                    <div class="col-md-5">
                                                        <label class="col-sm-12 col-form-label text-left pl-1">Maximum Limit</label>
                                                    </div>
                                                    <div class="col-md-7 input-group">
                                                        <span class="input-group-prepend">
                                                            <span class="input-group-text">OMR</span>
                                                        </span>
                                                        <input type="number" class="form-control" id="txtPPDMaximumLimit" name="PPDMaximumLimit" placeholder="Enter Maximum Amount">
                                                    </div>
                                                </div>
                                                <div class="row m-b-15 m-t-15">
                                                    <div class="col-md-5">
                                                        <label class="col-sm-12 col-form-label text-left pl-1">Waiting Period in Days</label>
                                                    </div>
                                                    <div class="col-md-7 input-group">

                                                        <input type="text" class="form-control" id="txtPPDWaitingYears" name="PPDWaitingYears" placeholder="Waiting Period in Days">
                                                        <span class="input-group-append">
                                                            <span class="input-group-text">Days</span>
                                                        </span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row m-b-15 m-t-15">
                                        <div class="col-md-3">
                                            <input type="checkbox" id="TotalDisability" value="Y" name="TTD" />
                                            <label for="TotalDisability">Temporary Total Disability (TTD)</label>
                                        </div>
                                        <div class="col-md-7 demo-section k-content">
                                            <div id="TotalDisabilityContent" style="display: none;">
                                                <div class="demo-radio-button d-flex justify-content-start">
