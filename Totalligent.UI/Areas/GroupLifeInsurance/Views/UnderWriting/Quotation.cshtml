                        item["WaitingPeriodinYears"] = $.trim($("#" + arrAdditionContent[i].id.replace("dynTitle", "WaitingPeriodinYears")).val()) == "" ? "0" : $.trim($("#" + arrAdditionContent[i].id.replace("dynTitle", "WaitingPeriodinYears")).val());
                        AdditionalCover.push(item);
                    }

                }
            }

            $("#hdnOtherAdditionalCover").val(JSON.stringify(AdditionalCover));
        }

        if (arrDynExclusions.length > 0) {
            for (var i = 0; i < arrDynExclusions.length; i++) {

                var id = arrDynExclusions[i].id;

                if (id.includes("dynTitle")) {

