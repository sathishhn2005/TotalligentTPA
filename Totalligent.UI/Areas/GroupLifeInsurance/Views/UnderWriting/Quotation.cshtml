        $(function () {

            $("#AccidentalDeath").click(function () {

                if ($(this).is(":checked")) {

                    $("#ACDContent").slideDown();

                } else {
                    $("#ACDContent").slideUp();
                }
            });
            $("#PermanentDisability").click(function () {
                
                if ($(this).is(":checked")) {
                    
                    $("#PermanentDisabilityContent").slideDown();

                } else {
                    $("#PermanentDisabilityContent").slideUp();
                }
            });
            $("#PermanentPartialDisability").click(function () {
                
                if ($(this).is(":checked")) {
                    
                    $("#PermanentPartialDisabilityContent").slideDown();

                } else {
                    $("#PermanentPartialDisabilityContent").slideUp();
                }
            });
            $("#TotalDisability").click(function () {
                
