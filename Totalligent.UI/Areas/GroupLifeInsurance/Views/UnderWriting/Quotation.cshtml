    function GetWCRateCalc(ClientName) {

        var param = { ClientCompanyName: ClientName };
        $.ajax({

            type: "GET",
            url: "/GroupLifeInsurance/Quotation/GetWCRate",
            dataType: "json",
            contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
            data: param,
            success: function (Result) {

                $('#objQuo_WCSumOfSalary').val(Result.WCSumOfSalary);
                $('#objQuo_WCEstimatedAnnualWages').val(Result.WCEstimatedAnnualWages);
                $('#objQuo_WCIncllevy').val(Result.WCIncllevy);
                $('#objQuo_WCGrossPremium').val(Result.WCGrossPremium);
                $('#objQuo_WCNetPremium').val(Result.WCNetPremium);
                $('#objQuo_WCPolicyFee1RO').val(Result.WCPolicyFee1RO);
                $('#objQuo_WCRate').val(Result.WCRate);
                var WC_BrokerName = $('#objQuo_BrokerName').val();
                var WC_BrokerCommission = $('#objQuo_BrokerCommission').val();
                var WC_NetPremium = Result.WCNetPremium;
                var WC_BrokerAmount = (parseFloat(WC_NetPremium) * parseFloat(WC_BrokerCommission)) / 100;

                $('#objQuo_WCBrokerName').val(WC_BrokerName);
                $('#objQuo_WCBrokerCommission').val(WC_BrokerCommission);
                $('#objQuo_WCBrokerage').val(WC_BrokerAmount);
