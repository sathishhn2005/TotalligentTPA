@{
    ViewBag.Title = "Masters";
    Layout = "~/Areas/GroupLifeInsurance/Views/Shared/_Layout.cshtml";
}
@*@Scripts.Render("~/Content/vendor/jquery-3.3.1/jquery-3.3.1.js");*@
<div class="row">
    <div class="col-md-12">
        <!-- Step wizard -->
        <div class="box box-default">
            <!-- /.box-header -->
            <div class="box-body wizard-content">
                <form id="formQuotation" action="#" class="tab-wizard wizard-circle">
                    <!-- Step 1 -->
                    <h6>Client Information</h6>
                    <section>
                        <div class="row">
                            <div class="hide">

                                @if (Model != null)
                                {@(Html.Kendo().Editor()
                                                            .Name("QuotationId")
                                                            .HtmlAttributes(new { style = "width: 100%; height:840px", aria_label = "editor" }))}
                            </div>
                            <div class="col-md-10 m-b-15 m-t-15 offset-md-1">
                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-4">
                                        <label class="col-sm-12 col-form-label text-left">Insurance Company Name</label>
                                    </div>
                                    @*$("#LifeEmgAreaCoverage").data("kendoMultiSelect").dataItems()*@

                                    <div class="col-md-7 demo-section k-content">
                                        @(Html.Kendo().ComboBox()
                                            .Name("CIInsuranceCompanyName")
                                            .Placeholder(">> Select Insurance Company Name ")
                                            .BindTo(new List<string>() {
                                                "ICICI Lombard",
                                                "HDFC Prudential",
                                                "Bajaj Allianz Life Insurance Company",
                                            })
                                            .HtmlAttributes(new { style = "width: 100%;font-size: 1rem;color: #5c6975;" })
                                        )
                                    </div>
                                </div>
                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-4">
                                        <label class="col-sm-12 col-form-label text-left">Client Company Name</label>
                                    </div>
                                    <div class="col-md-8 demo-section k-content">

                                        @(Html.Kendo().ComboBox()
                                            .Name("CIClientCompanyName")
                                            .Placeholder(">> Select Client Company Name ")
                                            .HtmlAttributes(new { style = "width: 100%;font-size: 1rem;color: #5c6975;", autocomplete = "off " })
                                        )
                                    </div>
                                </div>
                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-4">
                                        <label class="col-sm-12 col-form-label text-left">Scope of Coverage</label>
                                    </div>
                                    <div class="col-md-8 demo-section k-content">
                                        @Html.TextBox("SCoverage", "", new { @class = "form-control w-full", @placeholder = "Enter Scope of Coverage" })
                                    </div>
                                </div>
                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-4">
                                        <label class="col-sm-12 col-form-label text-left">Jurisdiction</label>
                                    </div>
                                    <div class="col-md-8 demo-section k-content">
                                        @Html.TextBox("Jurisdiction", "", new { @class = "form-control w-full", @placeholder = "Enter Jurisdiction" })
                                    </div>
                                </div>
                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-4">
                                        <label class="col-sm-12 col-form-label text-left">Address</label>
                                    </div>
                                    <div class="col-md-8 demo-section k-content">
                                        @Html.TextBox("Address", "", new { @class = "form-control w-full", @placeholder = "Enter Address" })
                                    </div>
                                </div>

                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-4">
                                        <label class="col-sm-12 col-form-label text-left">City</label>
                                    </div>
                                    <div class="col-md-8 demo-section k-content">
                                        @Html.TextBox("City", "", new { @class = "form-control w-full", @placeholder = "Enter City" })
                                    </div>
                                </div>
                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-4">
                                        <label class="col-sm-12 col-form-label text-left">KYC Details</label>
                                    </div>
                                    <div class="col-md-8 demo-section k-content">
                                        @Html.TextBox("KYCDetails", "", new { @class = "form-control w-full", @placeholder = "Enter KYC Details" })
                                    </div>
                                </div>
                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-4">
                                        <label class="col-sm-12 col-form-label text-left">Bank Name</label>
                                    </div>
                                    <div class="col-md-8 demo-section k-content">
                                        @Html.TextBox("BANKName", "", new { @class = "form-control w-full", @placeholder = "Enter Bank Name" })
                                    </div>
                                </div>

                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-4">
                                        <label class="col-sm-12 col-form-label text-left">IFSC Code</label>
                                    </div>
                                    <div class="col-md-8 demo-section k-content">
                                        @Html.TextBox("IFSCCODE", "", new { @class = "form-control w-full", @placeholder = "Enter IFSC Code" })
                                    </div>
                                </div>
                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-4">
                                        <label class="col-sm-12 col-form-label text-left">Branch</label>
                                    </div>
                                    <div class="col-md-8 demo-section k-content">
                                        @Html.TextBox("Branch", "", new { @class = "form-control w-full", @placeholder = "Enter Branch" })
                                    </div>
                                </div>
                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-4">
                                        <label class="col-sm-12 col-form-label text-left">Period of Insurance</label>
                                    </div>
                                    <div class="col-md-8 demo-section k-content">
                                        @Html.TextBox("PeriodInsurance", "", new { @class = "form-control w-full", @placeholder = "Enter Period of Insurance" })
                                    </div>
                                </div>
                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-4">
                                        <label class="col-sm-12 col-form-label text-left">Category</label>
                                    </div>
                                    <div class="col-md-8  ">
                                        <div class="row">
                                            <div class="col-md-12 d-flex">
                                                @Html.TextBox("CategoryName", "", new { @class = "form-control w-full", @placeholder = "Enter Category Name" })
                                                <button type="button" class="btn btn-primary ml-3"><i class="mdi mdi-check"></i></button>
                                            </div>
                                            <div class="col-md-12">
                                                <div class="table-responsive">
                                                    <table class="table table-hover table-sm">
                                                        <tr>
                                                            <th>Category Name</th>
                                                            <th>Actions</th>
                                                        </tr>
                                                        <tr>
                                                            <td>Gold Category</td>
                                                            <td>
                                                                <i class="mdi mdi-pencil hov-pointer text-primary font-size-16 mx-2"></i>
                                                                <i class="mdi mdi-delete hov-pointer text-danger font-size-16 mx-2"></i>
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td>Sliver Category</td>
                                                            <td>
                                                                <i class="mdi mdi-pencil hov-pointer text-primary font-size-16 mx-2"></i>
                                                                <i class="mdi mdi-delete hov-pointer text-danger font-size-16 mx-2"></i>
                                                            </td>
                                                        </tr>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
