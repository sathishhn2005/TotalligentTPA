    function bindCategoryType(loadType) {
        if ($("#hdnVariableTypeSumContent").val() != "") {
            var arrVal = JSON.parse($("#hdnVariableTypeSumContent").val());
            var arrCategory = arrVal.length == 0 ? [] : arrVal;
            var txtCategoryName;
            if (loadType == "addCategory") {
                txtCategoryName = $.trim($("#CategoryName").val());

                if (txtCategoryName == "") {
                    alert("Please Enter CategoryName");
                    return false;
                }
                else {
                    arrCate = jQuery.grep(arrVal, function (val, n) {
                        return ($.trim(val.CategoryName).toUpperCase() == $.trim(txtCategoryName).toUpperCase());
                    });

                    if (arrCate.length == 0) {
                        arrCategory.push({ CategoryName: txtCategoryName, Salary: "0.00", NoOfTimes: "0.00", SumAssured: "0.00", ClientRate: "0.00", NetPremium: "0.00" });
                    }
                    else {
                        alert("Please Check,This CategoryName already exists");
                        return false;
                    }

                }
            }

            $("#divCategoryType").empty();

            var tbl = '<table id="tblCategoryType" class="table table-hover table-sm"><tr><th>Category Name</th><th>Actions</th> </tr>';
            $.each(arrCategory, function (index, value) {

                tbl += '<tr><td>' + value.CategoryName + '</td><td><i class="mdi mdi-delete hov-pointer text-danger font-size-16 mx-2" onclick="DeleteCategoryType(this,' + "'" + value.CategoryName + "'" + ')"></i></td> </tr>';

            });

            tbl += '</table>';

            $("#divCategoryType").append(tbl);
            $("#hdnVariableTypeSumContent").val(JSON.stringify(arrCategory));
            $("#CategoryName").val('');

            BindPremiumVariableSumAssuredTbl(arrCategory);
        }
    }
