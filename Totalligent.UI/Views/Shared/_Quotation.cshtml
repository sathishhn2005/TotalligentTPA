
<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-header card-header-icon card-header-rose">
                <div class="card-icon">
                    <i class="material-icons">assignment</i>
                </div>
                <h4 class="card-title ">Quotation</h4>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-sm-12">
                        <!--      Wizard container        -->
                        <div class="wizard-container">
                            <div class="card-wizard" data-color="rose" id="wizardProfile">
                                <form action="" method="">
                                    <!--        You can switch " data-color="primary" "  with one of the next bright colors: "green", "orange", "red", "blue"       -->
                                    <div class="card-header text-center p-t-0">
                                        <h3 class="card-title">
                                            Quotation Builder
                                        </h3>
                                    </div>
                                    <div class="wizard-navigation">
                                        <ul class="nav nav-pills">
                                            <li class="nav-item">
                                                <a class="nav-link active" href="#Basic" data-toggle="tab" role="tab">
                                                    Basic Details
                                                </a>
                                            </li>
                                            <li class="nav-item">
                                                <a class="nav-link" href="#Benefits" data-toggle="tab" role="tab">
                                                    General Benefits
                                                </a>
                                            </li>
                                            <li class="nav-item">
                                                <a class="nav-link" href="#Inpatient" data-toggle="tab" role="tab">
                                                    Inpatient
                                                </a>
                                            </li>
                                            <li class="nav-item">
                                                <a class="nav-link" href="#Outpatient" data-toggle="tab" role="tab">
                                                    Out Patient & Daycare
                                                </a>
                                            </li>

                                            <li class="nav-item">
                                                <a class="nav-link" href="#ElectiveBenefits" data-toggle="tab" role="tab">
                                                    Elective Benefits
                                                </a>
                                            </li>
                                            <li class="nav-item">
                                                <a class="nav-link" href="#Optional" data-toggle="tab" role="tab">
                                                    Optional Benefits
                                                </a>
                                            </li>
                                            <li class="nav-item">
                                                <a class="nav-link" href="#Additional" data-toggle="tab" role="tab">
                                                    Additional Benefits
                                                </a>
                                            </li>
                                        </ul>
                                    </div>
                                    <div class="card-body">
                                        <div class="tab-content">
                                            <div class="tab-pane active" id="Basic">
                                                <div class="col-md-10 m-b-15 m-t-15 offset-md-1">
                                                    <div class="row m-b-15 m-t-15">
                                                        <div class="col-md-5">
                                                            <label class="col-sm-12 col-form-label text-left">Insurance Company Name</label>
                                                        </div>
                                                        <div class="col-md-7 demo-section k-content">
                                                            @(Html.Kendo().ComboBox()
                                                            .Name("ICName")
                                                            .Placeholder(">> Select Insurance Company Name ")
                                                            .BindTo(new List<string>() {
                                                                "ICICI Lombard",
                                                                "HDFC Prudential",
                                                                "Bajaj Allianz Life Insurance Company",
                                                            })
                                                            .HtmlAttributes(new { style = "width: 100%;font-size: 12px;" })
                                                            )
                                                        </div>
                                                    </div>
                                                    <div class="row m-b-15 m-t-15">
                                                        <div class="col-md-5">
                                                            <label class="col-sm-12 col-form-label text-left">Client Company Name</label>
                                                        </div>
                                                        <div class="col-md-7 demo-section k-content">
                                                            @(Html.Kendo().ComboBox()
                                                                .Name("CCName")
                                                                .Placeholder(">> Select Client Company Name ")
                                                                .BindTo(new List<string>() {
                                                                    "CTS",
                                                                    "TCS",
                                                                    "Dun & Bradstreet",
                                                                })
                                                                                                                .HtmlAttributes(new { style = "width: 100%;font-size: 12px;", autocomplete = "off " })
                                                            )
                                                        </div>
                                                    </div>
                                                    <div class="row m-b-15 m-t-15">
                                                        <div class="col-md-5">
                                                            <label class="col-sm-12 col-form-label text-left">Producer Type</label>
                                                        </div>
                                                        <div class="col-md-7 demo-section k-content">
                                                            @(Html.Kendo().DropDownList()
                                                            .Name("ProviderName")
                                                            .BindTo(new List<SelectListItem>()
                                                            {
                                                                new SelectListItem() {
                                                                    Text = "Direct", Value = "1"
                                                                },
                                                                new SelectListItem() {
                                                                    Text = "Broker", Value = "2"
                                                                },
                                                                new SelectListItem() {
                                                                    Text = "Agent", Value = "3"
                                                                }
                                                            })
                                                            .HtmlAttributes(new { style = "width: 100%;font-size: 12px;" })
                                                            .Events(e =>
                                                            {
                                                                e.Select("onProviderNameSelect");
                                                            })
                                                        )
                                                        </div>
                                                    </div>
                                                    <div class="row m-b-15 m-t-15">
                                                        <div class="col-md-5">
                                                            <label class="col-sm-12 col-form-label text-left">Select Producer</label>
                                                        </div>
                                                        <div class="col-md-7 demo-section k-content">
                                                            @(Html.Kendo().ComboBox()
                                                                .Name("BrokerName")
                                                                .Placeholder(">> Select Broker Name ")
                                                                .BindTo(new List<SelectListItem>()
                                                                {
                                                                    new SelectListItem() {
                                                                        Text = "OSI Broker", Value = "1"
                                                                    },
                                                                    new SelectListItem() {
                                                                        Text = "TTK Broker", Value = "2"
                                                                    }
                                                                })
                                                                .Enable(false)
                                                                .HtmlAttributes(new { style = "width: 100%;font-size: 12px;" })
                                                                .Events(e =>
                                                                {
                                                                    e.Select("onBrokerNameSelect");
                                                                })
                                                            )
                                                        </div>
                                                    </div>
                                                    <div class="row m-b-15 m-t-15">
                                                        <div class="col-md-5">
                                                            <label class="col-sm-12 col-form-label text-left">Select Commission</label>
                                                        </div>
                                                        <div class="col-md-4 demo-section k-content">
                                                            @(Html.Kendo().DropDownList()
                                                                .Name("BrokerCommission")
                                                                .BindTo(new List<string>() {
                                                                    "3%",
                                                                    "5%",
                                                                    "10%",
                                                                    "15%",
                                                                    "Add New"
                                                                })
                                                                .Enable(false)
                                                                .HtmlAttributes(new { style = "width: 100%;font-size: 12px;" })
                                                                .Events(e =>
                                                                {
                                                                    e.Select("onBrokerCommissionSelect");
                                                                })
                                                            )
                                                        </div>
                                                        <div class="col-md-3 demo-section k-content">
                                                            <input id="custom_Percentage" type="text" value="" disabled class="form-control" placeholder="Custom Percentage">
                                                        </div>
                                                    </div>
                                                    <div class="row m-b-15 m-t-15">
                                                        <div class="col-md-5">
                                                            <label class="col-sm-12 col-form-label text-left">Number of Plan Categories</label>
                                                        </div>
                                                        <div class="col-md-7 demo-section k-content">
                                                            @(Html.Kendo().DropDownList()
                                                                .Name("NoofCategories")
                                                                .BindTo(new List<string>() {
                                                                    "1",
                                                                    "2",
                                                                    "3",
                                                                    "4",
                                                                    "5"
                                                                })
                                                                .HtmlAttributes(new { style = "width: 100%;font-size: 12px;" })
                                                            )
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
