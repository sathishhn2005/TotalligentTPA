                                                <div class="col-md-5">
                                                    <label class="col-sm-12 col-form-label text-left">Medical Provider Network</label>
                                                </div>
                                                <div class="col-md-3 demo-section k-content">
                                                    @(Html.Kendo().DropDownList()
                                                                                                                                        .Name("GBMedicalProviderNetwork")
                                                                                                                                        .BindTo(new List<string>
                                                                                                                                            () {
                                                                                                                                                                                                                                                                                                                                                                                                            ">> Select Elective Treatement",
                                                                                                                                                                                                                                                                                                                                                                                                            "BAND - A",
                                                                                                                                                                                                                                                                                                                                                                                                            "BAND - B",
                                                                                                                                                                                                                                                                                                                                                                                                            "BAND - C",
                                                                                                                                                                                                                                                                                                                                                                                                            "Create Band",
                                                                                                                                            })
                                                                                                                                            .HtmlAttributes(new { style = "width: 100%;font-size: 1rem;color: #5c6975;" })
                                                    )
                                                </div>
                                                <div class="col-md-4 demo-section k-content">
                                                    @(Html.Kendo().DropDownList()
                                                                                                                                        .Name("AllHospital")
                                                                                                                                        .BindTo(new List<string>
                                                                                                                                        () {
                                                                                                                                                                                                                                                                                                                                                                                                    ">> Add Specific Hospital / Clinic",
                                                                                                                                                                                                                                                                                                                                                                                                    "Fortis Hospital",
                                                                                                                                                                                                                                                                                                                                                                                                    "Shanthi Hospital",
                                                                                                                                                                                                                                                                                                                                                                                                    "Stanely Hospital",
                                                                                                                                                                                                                                                                                                                                                                                                    "Goverment Hospital",                                                                                                            })
                                                                                                                                        .HtmlAttributes(new { style = "width: 100%;font-size: 1rem;color: #5c6975;" })
                                                    )
                                                </div>
                                            </div>
                                            <div class="row m-b-15 m-t-15">
                                                <div class="col-md-5">
                                                    <label class="col-sm-12 col-form-label text-left">Scope of Cover</label>
                                                </div>
                                                <div class="col-md-7 demo-section k-content col-form-label">
                                                    <div class="form-check dis-inline m-l-10">
                                                        <div class="demo-radio-button">
                                                            <input name="GBScopeOfCover" type="radio" id="InPatient" class="with-gap radio-col-primary" checked />
                                                            <label for="InPatient">In Patient</label>
                                                            <input name="GBScopeOfCover" type="radio" id="OutPatient" class="with-gap radio-col-primary" />
                                                            <label for="OutPatient">Out Patient</label>
                                                            <input type="checkbox" id="Optional" />
                                                            <label for="Optional">Optional / Additional</label>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row m-b-15">
                                                <div class="col-md-5">
                                                    <label class="col-sm-12 col-form-label text-left">Area of Coverage</label>
                                                </div>
                                                <div class="col-md-7 demo-section k-content">
                                                    @(Html.Kendo().MultiSelect()
                                                                                                                                                .Name("GBAreaOfCoverage")
                                                                                                                                                .AutoClose(false)
                                                                                                                                                .Placeholder(">> Select Area Coverage...")
                                                                                                                                                .BindTo(new List<string>() {
                                                                                                                                                                                                                                                                                                                                                                                                                "India",
                                                                                                                                                                                                                                                                                                                                                                                                                "Dubai",
                                                                                                                                                                                                                                                                                                                                                                                                                "kuwait",
                                                                                                                                                                                                                                                                                                                                                                                                                "England",
                                                                                                                                                                                                                                                                                                                                                                                                                "Australia",
                                                                                                                                                                                                                                                                                                                                                                                                                "America",
                                                                                                                                                                                                                                                                                                                                                                                                                "South Africa",
                                                                                                                                                                                                                                                                                                                                                                                                                "Brazil",
                                                                                                                                                })
                                                                                                                                                .HtmlAttributes(new { style = "width: 100%;font-size: 14px;" })
                                                    )
                                                </div>
                                            </div>
                                            <div class="row m-b-15 m-t-15">
                                                <div class="col-md-5">
                                                    <label class="col-sm-12 col-form-label text-left">Person Covered</label>
                                                </div>
                                                <div class="col-md-7 demo-section k-content col-form-label">
                                                    <div class="form-check dis-inline m-l-10">
                                                        <div class="demo-radio-button">
                                                            <input name="GBPersonCovered" type="checkbox" id="Employee" checked />
                                                            <label for="Employee">Employee</label>
                                                            <input name="GBPersonCovered" type="checkbox" id="Spouse" />
                                                            <label for="Spouse">Spouse</label>
                                                            <input name="GBPersonCovered" type="checkbox" id="Childrens" />
                                                            <label for="Childrens">Childrens</label>
                                                            <input name="GBPersonCovered" type="checkbox" id="Parents" />
                                                            <label for="Parents">Parents</label>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row m-b-15">
                                                <div class="col-md-5">
                                                    <label class="col-sm-12 col-form-label text-left">Annual Benefits Limit</label>
                                                </div>
                                                <div class="col-md-7 demo-section k-content">
                                                    @(Html.Kendo().DropDownList()
                                                                                                                                                        .Name("GBAnnualBenefitsLimit")
                                                                                                                                                        .BindTo(new List<string>
                                                                                                                                                            () {
                                                                                                                                                                                                                                                                                                                                                                                                                            ">> Select Annual Benefits Limit",
                                                                                                                                                                                                                                                                                                                                                                                                                            "10000",
                                                                                                                                                                                                                                                                                                                                                                                                                            "20000",
                                                                                                                                                                                                                                                                                                                                                                                                                            "50000",
                                                                                                                                                                                                                                                                                                                                                                                                                            "100000",
                                                                                                                                                                                                                                                                                                                                                                                                                            "150000",
                                                                                                                                                            })
                                                                                                                                                            .HtmlAttributes(new { style = "width: 100%;font-size: 1rem;color: #5c6975;" })
                                                    )
                                                </div>
                                            </div>
                                            @*--sathish*@
                                            <div class="row m-b-15 m-t-15">
                                                <label class="col-sm-12 col-form-label text-left font-weight-bold">Sum Assured</label>
                                            </div>
                                            <div class="row m-b-15 m-t-15">
                                                <div class="col-md-5">
                                                    <label class="col-sm-12 col-form-label text-left">Premium</label>
                                                </div>
                                                <div class="col-md-7 demo-section k-content">
                                                    @*<input class="form-control" onchange="CalcRenewalAdmin()" name="GBPremium" id="GBPremium" />*@
                                                    @(Html.Kendo().NumericTextBox()
.Name("GBPremium")
.Events(e => e
.Change("CalcRenewalAdmin()"))
.HtmlAttributes(new { style = "width: 100%" })
                                                    )


                                                </div>

                                            </div>
                                            <div class="row m-b-15 m-t-15">
                                                <div class="col-md-5">
                                                    <label class="col-sm-12 col-form-label text-left">Insurance levy @@0.55%</label>
                                                </div>
                                                <div class="col-md-7 demo-section k-content">
                                                    @*<input class="form-control" readonly name="GBInsuranceLevy" id="GBInsuranceLevy" />*@

                                                    @(Html.Kendo().TextBox()
.Name("GBInsuranceLevy")
.HtmlAttributes(new { @readonly = "readonly", style = "width: 100%" })
                                                    )

                                                </div>
                                            </div>
                                            <div class="row m-b-15 m-t-15">
                                                <div class="col-md-5">
                                                    <label class="col-sm-12 col-form-label text-left">Policy Fee</label>
                                                </div>

                                                <div class="col-md-7 demo-section k-content">
                                                    @*<input class="form-control" onchange="CalcTotalPremium()" name="GBPolicyFee" id="GBPolicyFee" />*@
                                                    @(Html.Kendo().NumericTextBox()
.Name("GBPolicyFee")
.Events(e => e
.Change("CalcTotalPremium()"))
.HtmlAttributes(new { style = "width: 100%" })
                                                    )
                                                </div>
                                            </div>
                                            <div class="row m-b-15 m-t-15">
                                                <div class="col-md-5">
                                                    <label class="col-sm-12 col-form-label text-left">Total Premium</label>
                                                </div>
                                                <div class="col-md-7 demo-section k-content">
                                                    @*<input class="form-control" readonly name="GBTotalPremium" id="GBTotalPremium" />*@

                                                    @(Html.Kendo().TextBox()
.Name("GBTotalPremium")
.HtmlAttributes(new { @readonly = "readonly", style = "width: 100%" })
                                                    )

                                                </div>
                                            </div>
                                            @*--sathish*@
                                            <div class="row m-b-15 m-t-15">
                                                <div class="col-md-5">
                                                    <label class="col-sm-12 col-form-label text-left">Per Claim Limit for In-Patient</label>
                                                </div>
                                                <div class="col-md-2">
                                                    <div class="togglebutton">
                                                        <button type="button" class="btn btn-toggle" id="GBIsPerClaimLimitIP" data-toggle="button" aria-pressed="false" autocomplete="off">
                                                            <div class="handle"></div>
                                                        </button>
                                                    </div>
                                                </div>
                                                <div class="col-md-5 demo-section k-content">
                                                    @*<input class="form-control" name="GBPerClaimLimitIP" placeholder="Enter Claim Limit for In-Patient" />*@
                                                    @(Html.Kendo().NumericTextBox()
.Name("GBPerClaimLimitIP")
.HtmlAttributes(new { style = "width: 100%" })
                                                    )
                                                </div>
                                            </div>
                                            <div class="row m-b-15 m-t-15">
                                                <div class="col-md-5">
                                                    <label class="col-sm-12 col-form-label text-left">Per Claim Limit for Out-Patient</label>
                                                </div>
                                                <div class="col-md-2">
                                                    <button type="button" class="btn btn-toggle" id="GBIsPerClaimLimitOP" data-toggle="button" aria-pressed="false" autocomplete="off">
                                                        <div class="handle"></div>
                                                    </button>
                                                </div>
                                                <div class="col-md-5 demo-section k-content">
                                                    @*<input class="form-control" name="GBPerClaimLimitOP" placeholder="Enter Claim Limit for Out-Patient" />*@
                                                    @(Html.Kendo().NumericTextBox()
.Name("GBPerClaimLimitOP")
.HtmlAttributes(new { style = "width: 100%" })
                                                    )
                                                </div>
                                            </div>
                                            <div class="row m-b-15 m-t-15">
                                                <div class="col-md-5">
                                                    <label class="col-sm-12 col-form-label text-left">Pre-Existing / Chronic Limit</label>
                                                </div>
                                                <div class="col-md-2">
                                                    <div class="togglebutton">
                                                        <label>
                                                            Full
                                                            <input type="checkbox">
                                                            <span class="toggle"></span>
                                                            Limit
                                                        </label>
                                                    </div>
                                                </div>
                                                <div class="col-md-5 demo-section k-content">
                                                    @*<input id="ChronicLimitInput" class="form-control" name="GBPreExisting_ChronicLimit" placeholder="Enter Chronic Limit" />*@
                                                    @(Html.Kendo().NumericTextBox()
.Name("GBPreExisting_ChronicLimit")
.HtmlAttributes(new { style = "width: 100%" })
                                                    )
                                                </div>
                                            </div>

                                            <div class="row m-b-15">
                                                <div class="col-md-5">
                                                    <label class="col-sm-12 col-form-label text-left">Geographical scope of Coverage for Life - Threatening Emergency Treatment</label>
                                                </div>
                                                <div class="col-md-3 demo-section k-content">
                                                    @(Html.Kendo().DropDownList()
                                                                                                                                                .Name("GBGeoThreateningEmergencyTreatment")
                                                                                                                                                .BindTo(new List<string>
                                                                                                                                                    () {
                                                                                                                                                                                                                                                                                                                                                                                                                    ">> Select Region",
                                                                                                                                                                                                                                                                                                                                                                                                                    "Worldwide",
                                                                                                                                                                                                                                                                                                                                                                                                                    "Specific Country",
                                                                                                                                                    })
                                                                                                                                                    .HtmlAttributes(new { style = "width: 100%;font-size: 1rem;color: #5c6975;" })
                                                    )
                                                </div>
                                                <div class="col-md-4 demo-section k-content">
                                                    @(Html.Kendo().MultiSelect()
                                                                                                                                                .Name("LifeEmgAreaCoverage")
                                                                                                                                                .AutoClose(false)
                                                                                                                                                .Placeholder(">> Select Specific Country...")
                                                                                                                                                .BindTo(new List<string>() {
                                                                                                                                                                                                                                                                                                                                                                                                                "India",
                                                                                                                                                                                                                                                                                                                                                                                                                "Dubai",
                                                                                                                                                                                                                                                                                                                                                                                                                "kuwait",
                                                                                                                                                                                                                                                                                                                                                                                                                "England",
                                                                                                                                                                                                                                                                                                                                                                                                                "Australia",
                                                                                                                                                                                                                                                                                                                                                                                                                "America",
                                                                                                                                                                                                                                                                                                                                                                                                                "South Africa",
                                                                                                                                                                                                                                                                                                                                                                                                                "Brazil",
                                                                                                                                                })
                                                                                                                                                .HtmlAttributes(new { style = "width: 100%;font-size: 14px;" })
                                                    )
                                                </div>
                                            </div>
                                            <div class="row m-b-15">
                                                <div class="col-md-5">
                                                    <label class="col-sm-12 col-form-label text-left">Geographical scope of Coverage for Elective Treatement</label>
                                                </div>
                                                <div class="col-md-3 demo-section k-content">
                                                    @(Html.Kendo().DropDownList()
                                                                                                                                                .Name("GBGeoElectiveTreatement")
                                                                                                                                                .BindTo(new List<string>
                                                                                                                                                    () {
                                                                                                                                                                                                                                                                                                                                                                                                                    ">> Select Region",
                                                                                                                                                                                                                                                                                                                                                                                                                    "Worldwide",
                                                                                                                                                                                                                                                                                                                                                                                                                    "Specific Country",
                                                                                                                                                    })
                                                                                                                                                    .HtmlAttributes(new { style = "width: 100%;font-size: 1rem;color: #5c6975;" })
                                                    )
                                                </div>
                                                <div class="col-md-4 demo-section k-content">
                                                    @(Html.Kendo().MultiSelect()
                                                                                                                                                .Name("ElectiveAreaCoverage")
                                                                                                                                                .AutoClose(false)
                                                                                                                                                .Placeholder(">> Select Specific Country...")
                                                                                                                                                .BindTo(new List<string>() {
                                                                                                                                                                                                                                                                                                                                                                                                                "India",
