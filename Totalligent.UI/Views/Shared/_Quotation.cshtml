                                                <div class="col-md-5">
                                                    <label class="col-sm-12 col-form-label text-left">Coverage Criteria for Treatment Outside Resident Country</label>
                                                </div>
                                                <div class="col-md-7 demo-section k-content">
                                                    <div class="form-check">
                                                        <div class="demo-radio-button">
                                                            <div>
                                                                <input name="GBGeoOutsideResidentCountry" type="radio" id="Reasonable" class="with-gap radio-col-primary" />
                                                                <label for="Reasonable">100% Subject to Reasonable & Customary Charges of Applicable Network</label>
                                                            </div>
                                                            <div>
                                                                <input name="GBGeoOutsideResidentCountry" type="radio" id="ActualCost" class="with-gap radio-col-primary" />
                                                                <label for="ActualCost">100% of Actual Cost Incurred</label>
                                                            </div>
                                                            <div>
                                                                <input name="GBGeoOutsideResidentCountry" type="radio" id="ActualCostI" class="with-gap radio-col-primary" />
                                                                <label for="ActualCostI">80% of Actual Cost Incurred</label>
