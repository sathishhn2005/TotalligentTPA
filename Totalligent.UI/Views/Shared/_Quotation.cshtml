@Scripts.Render("~/Content/vendor/jquery-3.3.1/jquery-3.3.1.js");
< div class="row">
    <div class="col-md-12">
        <!-- Step wizard -->
        <div class="box box-default">
            <!-- /.box-header -->
            <div class="box-body wizard-content">
                <form id="formQuotation" action="#" class="tab-wizard wizard-circle">
                    <!-- Step 1 -->
                    <h6>Client Information</h6>
                    <section>
                        <div class="row">
                            <div class="hide">
                                @if (Model != null)
{
                                    @(Html.Kendo().Editor()
                                                            .Name("QuotationId")
                                                            .HtmlAttributes(new { style = "width: 100%; height:840px", aria_label = "editor" }))
                                }
                            </div>
                            <div class="col-md-10 m-b-15 m-t-15 offset-md-1">
                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-5">
                                        <label class="col-sm-12 col-form-label text-left">Insurance Company Name</label>
                                    </div>
                                    @*$("#LifeEmgAreaCoverage").data("kendoMultiSelect").dataItems()*@

                                    <div class="col-md-7 demo-section k-content">
                                        @(Html.Kendo().ComboBox()
                                                                                                                                    .Name("CIInsuranceCompanyName")
                                                                                                                                    .Placeholder(">> Select Insurance Company Name ")
                                                                                                                                    .BindTo(new List<string>() {
                                                                                                                                                                                                                                                                                                                                                                "ICICI Lombard",
                                                                                                                                                                                                                                                                                                                                                                "HDFC Prudential",
                                                                                                                                                                                                                                                                                                                                                                "Bajaj Allianz Life Insurance Company",
                                                                                                                                    })
                                                                                                                                    .HtmlAttributes(new { style = "width: 100%;font-size: 1rem;color: #5c6975;" })
                                        )
                                    </div>
                                </div>
                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-5">
                                        <label class="col-sm-12 col-form-label text-left">Client Company Name</label>
                                    </div>
                                    <div class="col-md-7 demo-section k-content">
                                        @(Html.Kendo().ComboBox()
                                                                                                                                .Name("CIClientCompanyName")
                                                                                                                                .Placeholder(">> Select Client Company Name ")
                                                                                                                                .BindTo(new List<string>() {
                                                                                                                                                                                                                                                                                                                                    "CTS",
                                                                                                                                                                                                                                                                                                                                    "TCS",
                                                                                                                                                                                                                                                                                                                                    "Dun & Bradstreet",
                                                                                                                                })
                                                                                                                                                                                .HtmlAttributes(new { style = "width: 100%;font-size: 1rem;color: #5c6975;", autocomplete = "off " })
                                        )
                                    </div>
                                </div>
                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-5">
                                        <label class="col-sm-12 col-form-label text-left">Producer Type</label>
                                    </div>
                                    <div class="col-md-7 demo-section k-content">
                                        @(Html.Kendo().DropDownList()
                                                                                                                            .Name("CIProducerType")
                                                                                                                            .BindTo(new List<SelectListItem>()
                                                                                                                            {
                                                                                                                                                                                                                                                                                                                                new SelectListItem() {
                                                                                                                                                                                                                                                                                                                                    Text = "Direct", Value = "1"
                                                                                                                                                                                                                                                                                                                                },
                                                                                                                                                                                                                                                                                                                                new SelectListItem() {
                                                                                                                                                                                                                                                                                                                                    Text = "Broker", Value = "2"
                                                                                                                                                                                                                                                                                                                                },
                                                                                                                                                                                                                                                                                                                                new SelectListItem() {
                                                                                                                                                                                                                                                                                                                                    Text = "Agent", Value = "3"
                                                                                                                                                                                                                                                                                                                                }
                                                                                                                            })
                                                                                                                            .Enable(true)
                                                                                                                            .HtmlAttributes(new { style = "width: 100%;font-size: 1rem;color: #5c6975;" })
                                                                                                                            .Events(e =>
                                                                                                                            {
                                                                                                                                e.Select("onProviderNameSelect");
                                                                                                                            })
                                        )
                                    </div>
                                </div>
                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-5">
                                        <label class="col-sm-12 col-form-label text-left">Select Producer</label>
                                    </div>
                                    <div class="col-md-7 demo-section k-content">
                                        @(Html.Kendo().ComboBox()
                                                                                                                                .Name("CIProducerName")
                                                                                                                                .Placeholder(">> Select Producer Name ")
                                                                                                                                .BindTo(new List<SelectListItem>()
                                                                                                                                {
                                                                                                                                                                                                                                                                                                                                    new SelectListItem() {
                                                                                                                                                                                                                                                                                                                                        Text = "OSI Broker", Value = "1"
                                                                                                                                                                                                                                                                                                                                    },
                                                                                                                                                                                                                                                                                                                                    new SelectListItem() {
                                                                                                                                                                                                                                                                                                                                        Text = "TTK Broker", Value = "2"
                                                                                                                                                                                                                                                                                                                                    }
                                                                                                                                })
                                                                                                                                .Enable(true)
                                                                                                                                .HtmlAttributes(new { style = "width: 100%;font-size: 1rem;color: #5c6975;" })
                                                                                                                                .Events(e =>
                                                                                                                                {
                                                                                                                                    e.Select("onBrokerNameSelect");
                                                                                                                                })
                                        )
                                    </div>
                                </div>
                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-5">
                                        <label class="col-sm-12 col-form-label text-left">Select Commission</label>
                                    </div>
                                    <div class="col-md-4 demo-section k-content">
                                        @(Html.Kendo().DropDownList()
                                                                                                                                .Name("CISelectCommission")
                                                                                                                                .BindTo(new List<string>() {
                                                                                                                                                                                                                                                                                                                                    "3%",
                                                                                                                                                                                                                                                                                                                                    "5%",
                                                                                                                                                                                                                                                                                                                                    "10%",
                                                                                                                                                                                                                                                                                                                                    "15%",
                                                                                                                                                                                                                                                                                                                                    "Add New"
                                                                                                                                })
                                                                                                                                .Enable(true)
                                                                                                                                .HtmlAttributes(new { style = "width: 100%;font-size: 1rem;color: #5c6975;" })
                                                                                                                                .Events(e =>
                                                                                                                                {
                                                                                                                                    e.Select("onBrokerCommissionSelect");
                                                                                                                                })
                                        )
                                    </div>
                                    @*<div class="col-md-3 demo-section k-content">
                                            <input id="custom_Percentage" name="custom_Percentage" type="text" value="" class="form-control" placeholder="Custom Percentage">
                                        </div>*@
                                </div>
                                <div class="row m-b-15 m-t-15">
                                    <div class="col-md-5">
                                        <label class="col-sm-12 col-form-label text-left">Number of Plan Categories</label>
                                    </div>
                                    <div class="col-md-7 demo-section k-content">
                                        @(Html.Kendo().DropDownList()
                                                                                                                                .Name("CINoOfPlanCategories")
                                                                                                                                .BindTo(new List<string>() {
                                                                                                                                                                                                                                                                                                                                    "1",
                                                                                                                                                                                                                                                                                                                                    "2",
                                                                                                                                                                                                                                                                                                                                    "3",
                                                                                                                                                                                                                                                                                                                                    "4",
                                                                                                                                                                                                                                                                                                                                    "5"
                                                                                                                                })
                                                                                                                                .HtmlAttributes(new { style = "width: 100%;font-size: 1rem;color: #5c6975;" })
                                        )
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
                    <!-- Step 2 -->
                    <h6>General Benefits</h6>
                    <section>
                        <div class="row">
                            <div id="accordion" class="col-12">
                                <div class="box mb-0">
                                    <div class="box-header collapsed" data-toggle="collapse" href="#collapseOne">
                                        <a class="box-title">
                                            CATEGORY - 1
                                        </a>
                                        <i class="ti-angle-down float-right"></i>
                                    </div>
                                    <div id="collapseOne" class="box-body collapse" data-parent="#accordion">
                                        <div class="col-md-12 m-b-15 m-t-15">
