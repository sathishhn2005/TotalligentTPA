<script>
    function onProviderNameSelect(e) {
        if (e.item) {
            var dataItem = this.dataItem(e.item);
            if (dataItem.Value != 1) {
                $("#BrokerName").data("kendoComboBox").enable(true);
            }
            else {
                $("#BrokerName").data("kendoComboBox").enable(false);
            }
        }

    }
    function onBrokerNameSelect(e) {
        if (e.item) {
            var dataItem = this.dataItem(e.item);
            if (dataItem.Value > 0) {
                $("#BrokerCommission").data("kendoDropDownList").enable(true);
            }
            else {
                $("#BrokerCommission").data("kendoDropDownList").enable(false);
            }
        }

    }
    function onBrokerCommissionSelect(e) {
        if (e.item) {
            var dataItem = this.dataItem(e.item);
            if (dataItem == "Add New") {
                $("#custom_Percentage").removeAttr('disabled');
            }
            else {
                $("#custom_Percentage").attr('disabled');
            }
        }

    }
    
</script>